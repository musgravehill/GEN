$(document).ready(function(){$("#relResources .right-module").each(function(){var a=$(this).find(".imgover").attr("src");if(a){var b=a.substring(a.lastIndexOf("."),a.length);a=a.replace(b,"_o"+b);0==$(this).find(".imgoveron").length&&($(this).find(".right-module-img").append('\x3cimg class\x3d"imgoveron" src\x3d"'+a+'" style\x3d"display:none; border:none;" /\x3e'),$(this).hover(function(){$(this).find(".imgover").attr("style","display:none;");$(this).find(".imgoveron").attr("style","display:block; border:none;")},
function(){$(this).find(".imgover").attr("style","display:block; border:none;");$(this).find(".imgoveron").attr("style","display:none;")}))}})});