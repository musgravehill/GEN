Handlebars.registerHelper('getPartObject', function(context, options) {
    var cells = [], html, k;
    for (k in context) {
        if (context.hasOwnProperty(k)) {
            html = options.fn({
                key: k,
                value: context[k]
            }); 
            cells.push(html);
        }
    }
    return cells.join('');
});

Handlebars.registerHelper('ifCond', function(v1, v2, options) {
  if(v2 == "PIN" && (v1 != "" && v1 != 0)) {
    return options.fn(this);
  }
  if(v2 == "AREA" && (v1 != "" && v1 != 0)) {
	return options.fn(this);
  }
  if(v2 == "TYPE" && (v1 != "" && v1 != 0)) {
	return options.fn(this);
  }
  return options.inverse(this);
});

Handlebars.registerHelper('ifloop', function(v1, options) {
  if(v1 > 5) {
        return options.fn(this);
      }
      return options.inverse(this);
});

Handlebars.registerHelper('revenueCart', function(v1, options) {
  if(v1 == "Cart" && (v1 != "" && v1 != 0)) {
    return options.fn(this);
  }
    return options.inverse(this);
});
Handlebars.registerHelper('sampleCart', function(v1, options) {
 if(v1 == "SampleCart" && (v1 != "" && v1 != 0)) {
	return options.fn(this);
  }
  return options.inverse(this);
});
Handlebars.registerHelper('ifloop', function(v1, options) {
  if(v1 > 5) {
        return options.fn(this);
      }
      return options.inverse(this);
});
Handlebars.registerHelper('each-reverse', function(context) {
    var options = arguments[arguments.length - 1];
    var ret = '';

    if (context && context.length > 0) {
        for (var i = context.length - 1; i >= 0; i--) {
            ret += options.fn(context[i]);
        }
    } else {
        ret = options.inverse(this);
    }

    return ret;
});

$(document).on("click", ".checkUserLoggedIn", function() {
	var currentUser = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
    if(currentUser == 'anonymous') {
    	gigya.accounts.showScreenSet(aem.login.params);
        return false;
    }
});

$(document).on("click", ".checkUserLoginStatus", function() {
	var currentUser = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
    if(currentUser == 'anonymous') {
    	gigya.accounts.showScreenSet(aem.login.params);
        return false;
    }
    else {
		 populateQuoteScreenData();
		 $("#requestQuote").modal("show");
    }
});

function populateQuoteScreenData() {
	
	populateCountry('endCustomerCountryDiv', 'endCustomerCountry', 'endCustomerStateDiv', 'endCustomerState');
        populateProfileCountry('profileEndCustomerCountryDiv', 'profileEndCustomerCountry', 'profileEndCustomerStateDiv', 'profileEndCustomerState');

         $("#quoteRequestSubmitted").addClass("no-display");
        $("#requestQuote #form").removeClass("no-display");
    var state = (CQ_Analytics.PersonalInfoStoreMgr.getProperty("state") != "") ? CQ_Analytics.PersonalInfoStoreMgr.getProperty("state") : CQ_Analytics.PersonalInfoStoreMgr.getProperty("intState");

    if(CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId") == "anonymous"){
        return false;
    }
    if($('input[name=endCustomer]:checked', '#form').val()=='Me') {
		if (isEmpty(CQ_Analytics.PersonalInfoStoreMgr.getProperty("city")) || isEmpty(state) || isEmpty(CQ_Analytics.PersonalInfoStoreMgr.getProperty("country"))) {
			$('#profileEndCustomer').slideDown();
        } else {
            $('#profileEndCustomer').slideUp();
        }
	}
	$("#userDetails").html('<p>'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName")+' '+CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName")+'<br>\
		'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("email")+'<br>\
		'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("phone")+'<br>\
		'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("company",true)+'<br>\
		'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("city")+','+state+' '+CQ_Analytics.PersonalInfoStoreMgr.getProperty("postalCode")+'<br>\
		'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("country")+'</p>');
		
}



$(document).on("click", ".requestCompanyAccount", function() {
	var currentUser = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
    if(currentUser != 'anonymous') {
        $("#requestCompanyAccountModal").modal("show");
        return false;
    }
});

