//# sourceMappingURL=bootstrap-select.js.map
/**
 * Abstract localStorage to hide JSON operations.
 */
function Storage(key) {
    this.key = key;
    this._storage = window.localStorage;
}

Storage.prototype.set = function(value) {
    var str = JSON.stringify(value);
    return this._storage.setItem(this.key, str);
};

Storage.prototype.get = function() {
    var str = this._storage.getItem(this.key);
    return JSON.parse(str);
};


/**
 * Wrap an array (provides only a subset of operations)
 * to persist its content with the class Storage (see above)
 */
function ArrayStorage(key) {
    this._storage = new Storage(key);
}

ArrayStorage.prototype.get = function() {
    return this._storage.get() || [];
};

/**
 * decorator method to provide the JavaScript object to `func` and
 * then apply the changes to the storage
 */
ArrayStorage.prototype._wrap = function(func) {
    var arr = this.get();
    var output = func(arr);
    this._storage.set(arr);
    return output;
}

ArrayStorage.prototype.unshift = function(value) {
    return this._wrap(function(arr) {
        return arr.unshift(value);
    });
};

ArrayStorage.prototype.pop = function() {
    return this._wrap(function(arr) {
        return arr.pop();
    });
};

ArrayStorage.prototype.push = function(value) {
    return this._wrap(function(arr) {
        return arr.push(value);
    });
};


ArrayStorage.prototype.set = function(newArray) {
    return this._storage.set(newArray);
};
$(document).ready(function() {

    var lang = document.head.querySelector("[name=pathlanguage]").content;
    if (lang == "ja")
        lang = "jp";
    if (lang == "zh")
        lang = "cn";

    /**
     * fetch results from an URL and then append them to a container
     */
    function CrossSection() {
        var pageSize = "",
            noResultMessage = "",
            showMoreLabel = "",
            showLessLabel = "",
            crossTypeNoValueText = "",
            internalProductNoValueText = "";
        if ($('.cross-search-page-size').length > 0 && typeof $('.cross-search-page-size').attr('data-attr-value') != "undefined") {
            pageSize = $('.cross-search-page-size').attr('data-attr-value');
        }
        if ($('.cross-search-no-results-message').length > 0 && typeof $('.cross-search-no-results-message').attr('data-attr-value') != "undefined") {
            noResultMessage = $('.cross-search-no-results-message').attr('data-attr-value');
        }
        if ($('.cross-search-show-more-label').length > 0 && typeof $('.cross-search-show-more-label').attr('data-attr-value') != "undefined") {
            showMoreLabel = $('.cross-search-show-more-label').attr('data-attr-value');
        }
        if ($('.cross-search-show-less-label').length > 0 && typeof $('.cross-search-show-less-label').attr('data-attr-value') != "undefined") {
            showLessLabel = $('.cross-search-show-less-label').attr('data-attr-value');
        }
        if ($('.cross-search-cross-type-no-result-text').length > 0 && typeof $('.cross-search-cross-type-no-result-text').attr('data-attr-value') != "undefined") {
            crossTypeNoValueText = $('.cross-search-cross-type-no-result-text').attr('data-attr-value');
        }
        if ($('.cross-search-internal-product-no-result-text').length > 0 && typeof $('.cross-search-internal-product-no-result-text').attr('data-attr-value') != "undefined") {
            internalProductNoValueText = $('.cross-search-internal-product-no-result-text').attr('data-attr-value');
        }


        this.page = 1;
        this.perPage = pageSize != "" ? pageSize : 2;
        this.noResultMessage = noResultMessage != "" ? noResultMessage : CQ.I18n.get("crossSearchNoResultMessage");
        this.showMoreLabel = showMoreLabel != "" ? showMoreLabel : CQ.I18n.get("crossSearchShowmore");
        this.showLessLabel = showLessLabel != "" ? showLessLabel : CQ.I18n.get("backtomain");
        this.crossTypeNoValueText = crossTypeNoValueText != "" ? crossTypeNoValueText : CQ.I18n.get("crossSearchNoValueText");
        this.internalProductNoValueText = internalProductNoValueText != "" ? internalProductNoValueText : CQ.I18n.get("crossSearchNoInternalProductText");
        this.isShowingMore = false;
    }

    CrossSection.prototype.fetchFunction = function(searchtype) {

        var formData = {
            'keyword': searchtype,
            'page': this.page,
            'perPage': this.perPage
        };
        if (!$('.products-search-spinner').length) {
            $('.product-loader').prepend('<img class="products-search-spinner" src="/etc/designs/maximintegrated/_images/spiffy.gif" alt="Loading" style="display:block;padding-left: 50%;">')
        }
        $('.cross-search-parent-container').hide();
        return $.ajax({
            url: "/bin/SiliconExpertsController",
            method: "POST",
            data: formData,
            success: (function(response) {
                this.success(response);
                $('.product-loader .products-search-spinner').remove();
                $('.cross-search-parent-container').show();
                digitalCrossReferenceFunction(response);
            }).bind(this),
            error: function() {
                console.log('something went wrong in CrossSection.prototype.fetchFunction');
                $('.cross-search-parent-container').show();
            }
        });
    }

    CrossSection.prototype.fetchFunctionKBArticles = function(searchtype) {

        var formData = {
            'keyword': searchtype,
            'page': this.page,
            'perPage': this.perPage
        };
        if (!$('.products-search-spinner').length) {
            $('.product-loader').prepend('<img class="products-search-spinner" src="/etc/designs/maximintegrated/_images/spiffy.gif" alt="Loading" style="display:block;padding-left: 50%;">')
        }
        $('.cross-search-parent-container').hide();
        $.ajax({
            url: "/bin/KnowledgeArticlesServlet",
            method: "GET",
            data: formData,
            success: (function(response) {
                this.success(response);
                $('.product-loader .products-search-spinner').remove();
                document.getElementById("js-cross-reference-results").innerHTML = "";
                for (var i = 0; i < response.value.length; i++) {
                    var content = response.value[i].content;
					console.log(content);
                    var contentArray = content.split("</p>");
                    content = contentArray[0] + contentArray[1];
                    document.getElementById("js-cross-reference-results").innerHTML = document.getElementById("js-cross-reference-results").innerHTML + '<a target="_blank" href=https://maximsupport.microsoftcrmportals.com/en-us/knowledgebase/article/' + response.value[i].articlepublicnumber + '>' + response.value[i].title + '</a>' + "<p>" + content + "</p>";
                    $('.cross-search-parent-container').show();
                }
            }).bind(this),
            error: function() {
                console.log('something went wrong in CrossSection.prototype.fetchFunctionKBArticles');
                $('.cross-search-parent-container').show();
            }
        });
    }




    CrossSection.prototype.success = function(response) {

        if (typeof response != "undefined" && response != "") {
            this.processHeader(response);
            this.processCrossReferenceSearch(response);

            var totalPages = Math.ceil(response.total / response.pageSize);
            $("#page-numbers-crossreference").html("");
            if (response.total > 0) {
                $("#page-numbers-crossreference").append(createPages(response.offset, response.pageSize, response.total));
            } else {
                var keyword = $('#cross-search-bar-container #cross-search-bar').val();
                this.$container.append(this.noResultMessage + ' ' + CQ.I18n.get("for") + ' <b>"<span class="product-query">' + keyword + '</span>"</b>');
            }

        } else {
            var keyword = $('#cross-search-bar-container #cross-search-bar').val();
            this.$container.append(this.noResultMessage + ' ' + CQ.I18n.get("for") + '<b>"<span class="product-query">' + keyword + '</span>"</b>');
        }

    };

    CrossSection.prototype.processHeader = function(data) {
        this.template = this.crossreferenceHeader;
        this.$container = $('#search-results-type-title');

        this.$container.html("");
        data.isShowingMore = this.isShowingMore;
        data.showAllText = this.showMoreLabel;
        data.showLessText = this.showLessLabel;

        this.$container.append(this.render(data, this.isShowingMore));
    }

    CrossSection.prototype.render = function(results, isShowingMore) {
        return this.template(results, isShowingMore);
    };

    CrossSection.prototype.processCrossReferenceSearch = function(results) {
        
		if (window.location.href.indexOf("products") > -1) {
              this.template = this.crossreferenceSearch2;
            }
        else{
        	this.template = this.crossreferenceSearch;
        }


        this.$container = $('#js-cross-reference-results');

        this.$container.html("");

        if (typeof results.crossreferencedata != "undefined" && results.crossreferencedata.length > 0) {
            results.crossTypeNoValueText = this.crossTypeNoValueText;
            results.internalProductNoValueText = this.internalProductNoValueText;
            this.$container.append(this.render(results));
        }
    }


    CrossSection.prototype.crossreferenceHeader = function(results, isShowingMore) {

        var min = '',
            max = '';

        if (results.offset > 1)
            min = (results.offset - 1) * results.pageSize + 1;
        else
            min = 1;

        if (results.total < results.offset * results.pageSize)
            max = results.total;
        else
            max = results.offset * results.pageSize;

        var showAll = "";

        if (results.total > 0) {
            showAll = !results.isShowingMore ? ' | <a class="show-more-less showing-less">' + results.showAllText + '&nbsp;&gt;</a>' : ' | <a class="show-more-less showing-more">&nbsp;&lt; ' + results.showLessText + '</a>';
            return (
                '<p>' +
                '<span id="total-results-one">' + results.total + '</span> ' +
                'competitor part(s) </b> ' + CQ.I18n.get("resultsFor") + ' <b>"<span class="product-query">' + results.query + '</span>"</b> | ' +
                CQ.I18n.get("crossSearchViewing", [min, max, results.total]) +
                showAll +
                '</p>'
            );
        }

        return '';

    };

    CrossSection.prototype.crossreferenceSearch = function(results) {
        var txt = '';
        var langPrefix = "/" + lang;
        var crossReferenceData = results.crossreferencedata;
        crossReferenceData.forEach(function(data, i) {

            var statusJson = {
                "DEVELOPMENT": "Preliminary",
                "PRE-RELEASE": "Preliminary",
                "PRODUCTION": "Active",
                "MATURE": "NRND",
                "LAST TIME BUY": "Last Time Buy",
                "HI VOLUME ONLY": "Active",
                "OBSOLETE": "NLA"
            };
            txt += '<div id="TJA1050T" class="crossref-part">';

            txt += '<div class="row" id="CrossInfo">' +
                '<div class="js-section-header col-lg-12 crossref-cross"><b>' + data.partNumber + ' (' + data.manufacturer + ')</b><br>' + data.description + ' | RoHS: ' + data.roHSStatus + ' | Lifecycle: ' + data.lifecycle + '</div>' +
                '</div>';

            if (data.products.length > 0) {
                data.products.forEach(function(product, j) {
                    var topBorderClass = "";
                    if (j > 0) {
                        topBorderClass = "top-border";
                    }
                    txt += '<div class="row ' + topBorderClass + '" id="' + product.partNumber + '">';

                    txt +=
                        '<div class="col-md-3 col-sm-6">' +
                        '<span class="smallest">' + CQ.I18n.get('crossSearchPartNumber') + '</span>' +
                        '<p><a href="' + langPrefix + '/products/' + encodeURIComponent(product.root) + '">' + product.partNumber + '</a><br>' +
                        '<span class="smaller">' + product.description + '</span></p>' +
                        '</div>';


                    txt +=
                        '<div class="col-md-2 col-sm-6">' +
                        '<span class="smallest">' + CQ.I18n.get('crossSearchPriceUnit') + '</span>';


                    if (product.canSearch == "true" && product.productLifeCycle != "OBSOLETE" && product.productLifeCycle != "DEVELOPMENT" && product.productLifeCycle != "PRE-RELEASE") {
                        txt += '<p> <button type="button" class="btn btn-danger btn-xs rmargin12 uppercase addToCart" data-item-id="' + product.partNumber + '" onclick="buy_cart(\'' + product.partNumber + '\',\'part\',\'ProductSearch\',\'0\');">Buy</button> ' + product.baseprice + ' </p>';
                    }

                    txt += '</div>';

                    txt +=
                        '<div class="col-md-1 col-sm-6">' +
                        '<span class="smallest">' + CQ.I18n.get('crossSearchStatus') + '</span>';

                    if (product.productLifeCycle != "" && typeof statusJson[product.productLifeCycle] != "undefined")
                        txt += '<p>' + statusJson[product.productLifeCycle] + '</p>';
                    else
                        txt += '<p></p>';


                    if (product.productLifeCycle == "OBSOLETE") {
                        txt += '<br>' +
                            '<button type="button" class="btn btn-purple btn-xs rmargin12 uppercase" data-item-id="' + product.partNumber + '" onclick="redirect_rochester(\'' + product.partNumber + '\');">CHECK ROCHESTER &gt;</button>';
                    }

                    txt += '</div>';
                    txt +=
                        '<div class="col-md-1 col-sm-6">' +
                        '<span class="smallest">' + CQ.I18n.get('crossSearchCarrier') + '</span>' +
                        '<p>' + product.carrierType + '</p>' +
                        '</div>';

                    txt +=
                        '<div class="col-md-2 col-sm-6">' +
                        '<span class="smallest">' + CQ.I18n.get('crossSearchPackage') + '</span>' +
                        '<p>';
                    if (product.pinCount != undefined && product.pinCount != "undefined" && product.pinCount != "0") {
                        if (product.packageType != "")
                            txt += product.packageType + ';';

                        if (product.pinCount != "")
                            txt += ' ' + product.pinCount + 'Pin;';

                        if (product.packageLength != "" && product.packageWidth != "" && product.packageHeight != "")
                            txt += ' ' + product.packageLength + "mm(L) x " + product.packageWidth + "mm(W) x " + product.packageHeight + "mm(H)"
                    }
                    txt += '<br>' +
                        '<a href="' + langPrefix + '/emmi/content_lookup.cfm?Ac=S&amp;PN=' + product.partNumber + '"><img src="/etc/designs/maximintegrated/_images/icons/materials_analysis_icon.png" class="key-icon pointer materialsAnalysis"></a>';


                    if (typeof product.dataSheetURL != "undefined" && product.dataSheetURL != "") {
                        txt += '<a href="' + product.dataSheetURL + '"><img src="/etc/designs/maximintegrated/_images/icons/datasheet_download_icon.png" class="cart-icon"></a>';
                    }
                    txt += '<span class="smallest italic">' + CQ.I18n.get('seeText') + ' <a href="' + langPrefix + '/emmi/content_lookup.cfm?Ac=S&amp;PN=' + product.partNumber + '">' + CQ.I18n.get('crossSearchMaterialAnalysis') + '</a> ' + CQ.I18n.get('forRohs');
                    txt += '</div>';

                    txt +=
                        '<div class="col-md-1 col-sm-6">' +
                        '<span class="smallest">' + CQ.I18n.get('crossSearchCrossType') + '</span>';

                    if (product.type != "") {
                        txt += '<p><span data-toggle="tooltip" title="' + CQ.I18n.get('crossSearchCrossType' + product.type.replace("/", "")) + '" data-placement="bottom">' + product.type + ' <img src="/etc/designs/maximintegrated/_images/icons/info_icon.png" class="cart-icon"></span></p>';
                    } else {

                        txt += '<p>' + results.crossTypeNoValueText + '</p>';
                    }

                    txt += '</div>';

                    txt +=
                        '<div class="col-md-2 col-sm-6">' +
                        '<span class="smallest">' + CQ.I18n.get('crossSearchDifferences') + '</span>';

                    if (typeof product.comment != "undefined" && product.comment != "")
                        txt += '<p>' + product.comment + '</p>';
                    else
                        txt += '<p>' + CQ.I18n.get('crossSearchDifferenceNoShow') + '</p>';

                    txt += '</div></div>';


                });
            } else {
                txt += "<p>" + results.internalProductNoValueText + "</p>";
            }

            txt += '</div>';

        });
        //console.log(txt);
        return txt;
    };

 CrossSection.prototype.crossreferenceSearch2 = function(results) {
        var txt = '';
        var langPrefix = "/" + lang;
        var crossReferenceData = results.crossreferencedata;
        crossReferenceData.forEach(function(data, i) {

            var statusJson = {
                "DEVELOPMENT": "Preliminary",
                "PRE-RELEASE": "Preliminary",
                "PRODUCTION": "Active",
                "MATURE": "NRND",
                "LAST TIME BUY": "Last Time Buy",
                "HI VOLUME ONLY": "Active",
                "OBSOLETE": "NLA"
            };
            txt += '<div id="TJA1050T" class="crossref-part">';

			if(i==0){
				txt += '<div class="Product_tableHeader mt15">' +
					    '<h4 class="dark-gray">' + data.partNumber + ' (' + data.manufacturer + ')</h4>' +
         				'<p>' + data.description + ' | RoHS: ' + data.roHSStatus + ' | Lifecycle: ' + data.lifecycle + '</p>' +
       				'</div>';
			} else{
			txt += '<div class="Product_tableHeader mt30">' +
					    '<h4 class="dark-gray">' + data.partNumber + ' (' + data.manufacturer + ')</h4>' +
         				'<p>' + data.description + ' | RoHS: ' + data.roHSStatus + ' | Lifecycle: ' + data.lifecycle + '</p>' +
       				'</div>';
			}
			

            if (data.products.length > 0) {
	
				txt += '<table class="table table-responsive table-hover table-condensed maxim-table" id="tblPartsUsed">' +
         				'<tbody>' +
           					'<tr>' +
             					'<th class="header">' + CQ.I18n.get('crossSearchPartNumber') + '</th>' +
             					'<th class="header">' + CQ.I18n.get('crossSearchPriceUnit') + '</th>' +
             					'<th class="header">' + CQ.I18n.get('crossSearchStatus') + '</th>' +
             					'<th class="header">' + CQ.I18n.get('crossSearchCarrier') + '</th>' +
             					'<th class="header">' + CQ.I18n.get('crossSearchPackage') + '</th>' +
             					'<th class="header">' + CQ.I18n.get('crossSearchCrossType') + '</th>' +
             					'<th class="header">' + CQ.I18n.get('crossSearchDifferences') + '</th>' +
          					'</tr>';


                data.products.forEach(function(product, j) {
                    var topBorderClass = "";
                    if (j > 0) {
                        topBorderClass = "top-border";
                    }

					
					txt += '<tr>' +
								'<td><a href="' + langPrefix + '/products/' + encodeURIComponent(product.root) + '" name="&amp;lid=en_US_folder_ts_rel_products_tab_ti_devices_part_number"' +
			                               'data-di-id="di-id-c5fdd39d-9fa9e207">' + product.partNumber + '</a>&nbsp;' +
									'<p>' + product.description + '</p>' +
			             		'</td>' +
								'<td>';	
										
									if (product.canSearch == "true" && product.productLifeCycle != "OBSOLETE" && product.productLifeCycle != "DEVELOPMENT" && product.productLifeCycle != "PRE-RELEASE") {
				                        txt += '<button type="button" class="btn btn-outline-secondary prod-learn-more addToCart" data-item-id="' + product.partNumber + '" onclick="buy_cart(\'' + product.partNumber + '\',\'part\',\'ProductSearch\',\'0\');">Buy </button> ' + product.baseprice ;
				                    }

					txt += 		'</td>' +
								'<td>';
								
									if (product.productLifeCycle != "" && typeof statusJson[product.productLifeCycle] != "undefined")
				                        txt += '<p>' + statusJson[product.productLifeCycle] + '</p>';
				                    else
				                        txt += '<p></p>';
	
	
				                    if (product.productLifeCycle == "OBSOLETE") {
				                        txt += '<br>' +
				                            '<button type="button" class="btn btn-purple btn-xs rmargin12 uppercase" data-item-id="' + product.partNumber + '" onclick="redirect_rochester(\'' + product.partNumber + '\');">CHECK ROCHESTER &gt;</button>';
				                    }
			
					txt += 		'</td>' +
								'<td>' +
									'<p>' + product.carrierType + '</p>' +
								'</td>' +
								'<td>';
                    				if (product.pinCount != undefined && product.pinCount != "undefined" && product.pinCount != "0") {
				                        if (product.packageType != "")
				                            txt += product.packageType + ';';
				
				                        if (product.pinCount != "")
				                            txt += ' ' + product.pinCount + 'Pin;';
				
				                        if (product.packageLength != "" && product.packageWidth != "" && product.packageHeight != "")
				                            txt += ' ' + product.packageLength + "mm(L) x " + product.packageWidth + "mm(W) x " + product.packageHeight + "mm(H)"
				                    }
                    txt += 			'<p class="doc_reference">';
										
					                    if (typeof product.dataSheetURL != "undefined" && product.dataSheetURL != "") {
					                        txt += '<a href="' + product.dataSheetURL + '"><img src="/etc/designs/maximintegrated/_images/icons/document-sharp.svg" class="cart-icon mr5"></a>';
					                    }
                    txt += 				' <a href="' + langPrefix + '/emmi/content_lookup.cfm?Ac=S&amp;PN=' + product.partNumber + '">' +
												CQ.I18n.get('crossSearchMaterialAnalysis') + '</a> ' + CQ.I18n.get('forRohs');
                    txt += 			'</p>' +
								'</td>' +
								'<td>';
                        
									if (product.type != "") {
				                        txt += '<p><span data-toggle="tooltip" title="' + CQ.I18n.get('crossSearchCrossType' + product.type.replace("/", "")) + '" data-placement="bottom">' + product.type + ' <span class="info-icon-results">i</span></span></p>';
				                    } else {
				
				                        txt += '<p>' + results.crossTypeNoValueText + '</p>';
				                    }

                    txt += 		'</td>' +
								'<td>';
                       
				                    if (typeof product.comment != "undefined" && product.comment != "")
				                        txt += '<p>' + product.comment + '</p>';
				                    else
				                        txt += '<p>' + CQ.I18n.get('crossSearchDifferenceNoShow') + '</p>';

                    txt += 		'</td>' +
							'</tr>';
					


                });
            } else {
				
                txt += "<tr><p>" + results.internalProductNoValueText + "</p><tr>";
            }

			txt += '<tbody><table></div>';
           

        });
        //console.log(txt);
        return txt;
    };

    var crossSection = new CrossSection();

    $('#cross-search-bar-container #cross-search-form').submit(function() {
        var keyword = $('#cross-search-bar-container #cross-search-bar').val();
        if (typeof keyword != "undefined" && keyword != "") {
            crossSection.page = 1;
            updateURL(encodeURIComponent(keyword));

            if (window.location.href.indexOf("knowledge-base-search") > -1) {
                crossSection.fetchFunctionKBArticles(keyword);
            } else {
                crossSection.fetchFunction(keyword);
            }
        }
        return false;
    });

	$(document).on('click', '#cross-ref-btn', function() {
		var keyword = $('#cross-search-bar-container #cross-search-bar').val();
        if (typeof keyword != "undefined" && keyword != "") {
            crossSection.page = 1;
            updateURL(encodeURIComponent(keyword));

            if (window.location.href.indexOf("knowledge-base-search") > -1) {
                crossSection.fetchFunctionKBArticles(keyword);
            } else {
                crossSection.fetchFunction(keyword);
            }
        }
        return false;
	});

$('#form-group #prod-search-btn').submit(function() {
        var keyword = $('#form-group #prod-search').val();
        if (typeof keyword != "undefined" && keyword != "") {
            crossSection.page = 1;
            updateURL(encodeURIComponent(keyword));

            if (window.location.href.indexOf("knowledge-base-search") > -1) {
                crossSection.fetchFunctionKBArticles(keyword);
            } else {
                crossSection.fetchFunction(keyword);
            }
        }
        return false;
    });

    $(document).on('click', '.pagenumcrossreference', function() {
        crossSection.page = $(this).data('value');
        var keyword = $('#cross-search-bar-container #cross-search-bar').val();
        if (typeof keyword != "undefined" && keyword != "") {
            updateURL(encodeURIComponent(keyword));
            if (window.location.href.indexOf("knowledge-base-search") > -1) {
                crossSection.fetchFunctionKBArticles(keyword);
            } else {
                crossSection.fetchFunction(keyword);
            }
            return true;
        }
    });

    $(document).on('click', '.show-more-less', function() {
        var keyword = $('#cross-search-bar-container #cross-search-bar').val();
        if (typeof keyword != "undefined" && keyword != "") {
            crossSection.page = 1;
            if ($(this).hasClass('showing-less')) {
                if ($('.cross-search-show-more-count').length > 0)
                    crossSection.perPage = $('.cross-search-show-more-count').attr('data-attr-value');

                crossSection.isShowingMore = true;
                updateURL(encodeURIComponent(keyword));
                if (window.location.href.indexOf("knowledge-base-search") > -1) {
                    crossSection.fetchFunctionKBArticles(keyword);
                } else {
                    crossSection.fetchFunction(keyword);
                }
            } else {
                if ($('.cross-search-page-size').length > 0)
                    crossSection.perPage = $('.cross-search-page-size').attr('data-attr-value');
                crossSection.isShowingMore = false;
                updateURL(encodeURIComponent(keyword));
                if (window.location.href.indexOf("knowledge-base-search") > -1) {
                    crossSection.fetchFunctionKBArticles(keyword);
                } else {
                    crossSection.fetchFunction(keyword);
                }
            }
        }
    });
    $(window).on("popstate", function() {
        // if the state is the page you expect, pull the name and load it.
        if (history.state && "Cross Reference Search" === history.state.page) {
            $('#cross-search-bar-container #cross-search-bar').val(decodeURIComponent(history.state.name));
            crossSection.fetchFunction(decodeURIComponent(history.state.name));
        }
    });

    function updateURL(keyword) {
        var newURL = window.location.protocol + "//" + window.location.host + window.location.pathname + "?sp_q=" + keyword;
        var state = {
            name: keyword,
            page: 'Cross Reference Search'
        };
        window.history.pushState(state, "", newURL);
    }

    function createPages(current, count, total) {
        // following comments will use an example of 20 pages.
        var config = {
            displayed: 5,
        };

        var numberOfPages = Math.ceil(total / count);
        if (numberOfPages < config.displayed) {
            config.displayed = numberOfPages;
        }

        if (current > numberOfPages) {
            current = numberOfPages;
        }
        // find the middl-ish of the array
        var boundary = Math.ceil((config.displayed - 2) / 2);
        var start = current - boundary;
        // we don't want [-1, 0, 1, 2, 3]
        // when current = 1
        if (start < 1) {
            start = 1;
        }
        // same but for the last page
        if (start > Math.abs(numberOfPages - config.displayed)) {
            start = numberOfPages - config.displayed + 1;
        }

        var pages = [];

        // creating an array with current in the middle
        // current = 8 -> [6, 7, 8, 9, 10]
        for (var i = start; i < (start + config.displayed); i += 1) {
            pages.push(i);
        }

        // then replace both end of the array with first page and last page.
        // [1, 7, 8, 9, 20]
        if (pages[0] !== numberOfPages) {
            pages[pages.length - 1] = numberOfPages;
        }
        if (pages[0] !== 1) {
            pages[0] = 1;
        }

        var content = [];

        if (pages.length > 1) {
            pages.forEach(function(page) {
                var str = page;
                // make sure first and second items DO NOT follow each other != [1,2]
                // in that case, we append ... to the first page
                if (page === 1 && pages[1] !== 2) {
                    str += '...';
                }
                // same as above but for the last page (with our previous example: [19, 20])
                if (page === numberOfPages && pages[pages.length - 2] !== (numberOfPages - 1)) {
                    str = '...' + str;
                }
                if (page === current) {
                    str = '<b>' + page + '<b>';
                }
                content.push('<div class="page-number crossreference"><a target="#cross-search-bar-container" class="pagenumcrossreference" data-value="' + page + '">' + str + '</a></div>');
            });

            if (current > 1) {
                content.unshift('<div class="page-number crossreference"><a target="#cross-search-bar-container" class="pagenumcrossreference gray" data-value="' + (current - 1) + '">&lt; ' + CQ.I18n.get("previousPage") + '</a></div>');
            }

            if (current !== numberOfPages) {
                content.push('<div class="page-number crossreference"><a target="#cross-search-bar-container" class=" gray pagenumcrossreference" data-value="' + (current + 1) + '">' + CQ.I18n.get("nextPage") + ' &gt;</a></div>');
            }
        }

        return content;
    }
    var crossSearchContainer = $('#cross-search-bar-container #cross-search-bar');
    if ($(crossSearchContainer).length > 0 && $(crossSearchContainer).val() != "") {
        crossSection.page = 1;
        var keyword = $('#cross-search-bar-container #cross-search-bar').val();
        updateURL(encodeURIComponent(keyword));
        if (window.location.href.indexOf("knowledge-base-search") > -1) {
            crossSection.fetchFunctionKBArticles(keyword);
        } else {
            crossSection.fetchFunction(keyword);
        }
    }

    function digitalCrossReferenceFunction(data) {
        var digitalSearchData = {};
        window.digitalSearchData = {
            page: {
                pageInfo: {
                    onsiteSearchTerm: data.query,
                    onsiteSearchResults: data.total,
                    originPage: "Cross Reference"
                }
            }
        }
        if (typeof _satellite != "undefined") {
            _satellite.track("search-cross-reference");
        }
    }
	
	$('[data-toggle="tooltip"]').tooltip();

});
window.aem = window.aem || {};
window.formRegister = "";


window.aem.setProfilePreferencePopupFlag = function() {
   CQ_Analytics.ProfileDataMgr.setProperty("popupViewed",true);

}

window.aem.removeErrorCodeFromURL = function(errorCodeString) {

    var urlWihtoutParameter = location.protocol + '//' + location.host + location.pathname;

    if (errorCodeString != "") {
        if (typeof(history.pushState) != "undefined") {
            var obj = {
                Page: 'homepage',
                Url: urlWihtoutParameter
            };
            history.pushState(obj, obj.Page, obj.Url);
        } else {
            console.log("Browser does not support HTML5.");
        }
    }
}

window.aem.fireModelNSamlResponse=function(app, modal, callback) {

    if ((CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) === true || CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) == "true") && typeof gigya != "undefined") {
        modelNSamlResponse(callback);
    } else if (modal == "login") {
        gigya.accounts.showScreenSet(aem.login.params);
    }
}

window.aem.fireCRMSamlResponse=function(app, model, callback) {

    if ((CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) === true || CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) == "true") && typeof gigya != "undefined") {
        gigyaSamlResponse(callback);
    } else if (model == "login") {
        gigya.accounts.showScreenSet(aem.login.params);
    }
}

window.aem.hideSiteLoader=function() {
    if ($('#loading').length > 0 && $("#loading").css('display') != "none") {
        $('#loading').hide();
    }
}

window.aem.showSiteLoader=function() {
    if ($('#loading').length > 0 && $("#loading").css('display') == "none") {
        $('#loading').show();
    }
}

window.aem.getTodaysDate = function() {
	var today = new Date();
	var day = today.getDate();
	var month = today.getMonth()+1; //January is 0!
	var hours = today.getHours();
	var minutes = today.getMinutes();
	var seconds = today.getSeconds();
	var yyyy = today.getFullYear();
	var timeZone = today.getTime
	if(day<10)
		day='0'+day; 
	if(month<10)
		month='0'+month;
	if(hours<10)
		hours='0'+hours;
	if(minutes<10)
		minutes='0'+minutes;
	if(seconds<10)
		seconds='0'+seconds;

	return yyyy+"-"+month+"-"+day+"T"+hours+":"+minutes+":"+seconds+"Z";
	
}

window.aem.getCampaignValue = function(requestParameters) {
	
	if(requestParameters == undefined)
		return "";
	
	var utmSource = requestParameters.utm_source;
	var utmMedium = requestParameters.utm_medium;
	var utmCampaign = requestParameters.utm_campaign;
	var utmTerm = requestParameters.utm_term;
	var utmContent = requestParameters.utm_content;

	var campaignValue = "";
	
	if(utmSource != undefined && utmSource != "") {
		campaignValue = "utm_source="+utmSource;
	}
	if(utmMedium != undefined && utmMedium != "") {
		if(campaignValue.length > 0)
			campaignValue+="&";
		campaignValue+= "utm_medium="+utmMedium;
	}
	if(utmCampaign != undefined && utmCampaign != "") {
		if(campaignValue.length > 0)
			campaignValue+="&";
		campaignValue+= "utm_campaign="+utmCampaign;
	}
	if(utmTerm != undefined && utmTerm != "") {
		if(campaignValue.length > 0)
			campaignValue+="&";
		campaignValue+= "utm_term="+utmTerm;
	}
	if(utmContent != undefined && utmContent != "") {
		if(campaignValue.length > 0)
			campaignValue+="&";
		campaignValue+= "utm_content="+utmContent;
	}
	
	return campaignValue;
	
}
window.aem.actionBeforeRegisterScreenLoad = function(object) {
	
	aem.customGigyaPopupCloseEvent();
	
	
	var policyAcceptanceElement = $('#gigya-register-form input[name="data.maxim.policyacceptancetime"]');
	var campaignElement = $('#gigya-register-form input[name="data.maxim.eloquacampaignid"]');
	var requestParameters = parseQueryString(location.search.substring(1));
	
	if(typeof policyAcceptanceElement != "undefined") {
		
			$(policyAcceptanceElement).val(window.aem.getTodaysDate());
	}
	
	if(typeof campaignElement != "undefined") {
		$(campaignElement).val(window.aem.getCampaignValue(requestParameters));
	}
	
    $('select[name="data.maxim.country"]').on("change", function() {
        var countryCode = window.aem.getCountryCode($(this).val());

        if (countryCode != "") {
            if ($('input[name="data.maxim.countrycode"]').length > 0) {
                $('input[name="data.maxim.countrycode"]').val(countryCode);
            }
        }
    });
	
	var modelnApp = requestParameters.app;
	if(typeof modelnApp != "undefined" && modelnApp == "modeln"){
		$('input[name="data.maxim.ismodelnuser"]').val('true');
	}
	var currentLanguage = getUrlLanguage();
	if(currentLanguage == "ja") {
		$('select[name="data.maxim.country"]').val("JP");
		$('input[name="profile.locale"]').val("ja");
	} else if(currentLanguage == "zh") {
		$('select[name="data.maxim.country"]').val("CN");
		$('input[name="profile.locale"]').val("zh-cn");
	} else {
		$('select[name="data.maxim.country"]').val("US");
		$('input[name="profile.locale"]').val("en");
	}
    $('select[name="data.maxim.country"]').change();
	
	if(window.digitalDataSearch != undefined && object.currentScreen == "gigya-register-screen") {
		window.digitalDataSearch.form={ 
			"formName":"Create Account:New User Signup", 
			"event": "form-start"
		}

	}
	var url=window.location.href;
	var resource="";
    var utm_source = "";
    var utm_campaign = "";

	var hostname=window.location.host;
	var referUrl="";

	if(url.includes("login.html"))
	{

		var urlParams = new URLSearchParams(window.location.search);
		resource = urlParams.get('resource');
        utm_source = urlParams.get('utm_source');
        utm_campaign = urlParams.get('utm_campaign');        

        referUrl=location.protocol+"//"+hostname+resource;

        if(utm_source!=null){
            referUrl = updateQueryStringParameter(referUrl, "utm_source", utm_source);
        }

        if(utm_campaign!=null){
            referUrl = updateQueryStringParameter(referUrl, "utm_campaign", utm_campaign);
        }        

	}
	
	if(url.includes("en.html") && url.includes("app=legacy")&& (""!= document.referrer)){
		referUrl=document.referrer;
		
	}else if(url.includes("en.html") && url.includes("app=legacy")&& (""=== document.referrer)){
		referUrl=window.location.href;
	}

	$('input[name="data.maxim.registerPageName"]').val(referUrl);

}

window.aem.showProfilePopup = function () {
	
	if($('#myMaxDialog').length > 0) {
		$('#myMaxDialog').modal('hide'); 
	}
	aem.showSiteLoader();
	if(typeof gigya != "undefined" && typeof aem != "undefined" && typeof aem.profile != "undefined" && typeof aem.profile.params != "undefined") {
		gigya.accounts.showScreenSet(aem.profile.params);
	}
	
}
window.aem.getCountryCode = function(country) {
    var countryCode = "+1";
    if (country != "") {
        jQuery.ajax("/bin/getcountrycode", {
            data: {
                country: country
            },
            dataType: "text",
            type: "POST",
            async: false,
            global: false,
            success: function(data, code, jqXHR) {
                var resJson = JSON.parse(data);
                if (resJson.country == country && typeof resJson.country_code != "undefined") {
                    var country_code = resJson.country_code;
                    if (country_code != "")
                        countryCode = "+" + country_code;
                }
            },
            error: function() {
                console.log('System error occured. Not able to load country code. Default +1 will be applied');
            }
        })
    }
    return countryCode;

}


window.aem.checkUserAccountStatus = function() {
    var email = $('#gigya-login-form input[name=username]').val();
    var isAccountActive = true;
    if (email != "") {
        jQuery.ajax("/bin/check_gigya_user_status", {
            data: {
                gigyaId: email
            },
            dataType: "text",
            type: "POST",
            async: false,
            global: false,
            success: function(data, code, jqXHR) {
                var resJson = JSON.parse(data);
                if (resJson.account_status) {
                    var account_status = resJson.account_status;
                    if (account_status.isbanned || account_status.isstale || account_status.iszapped) {
                        if ($('.gigya-screen-dialog').length > -1) {
                            $('.gigya-screen-dialog').hide();
                        }
                        var bootBoxMsg = "";
                        if (account_status.isbanned) {
                            bootBoxMsg += 'Your account is banned. Please contact maxim Support';
                        } else if (account_status.iszapped) {
                            bootBoxMsg += 'Your account is zapped. Please contact maxim Support';
                        } else if (account_status.isstale) {
                            bootBoxMsg += 'Your account is stale. Please contact maxim Support';
                        }
                        digitalErrorFunction(bootBoxMsg);
                        bootbox.alert(bootBoxMsg);
                        isAccountActive = false;
                    }
                }
            },
            error: function() {
            	digitalErrorFunction('System error occured. Please try again later');
                bootbox.alert('System error occured. Please try again later');
            }
        })
    }
    return isAccountActive;
}

/*
 * Logic to clear aem session if Gigya session is out
 */
window.aem.checkAccountInfoResponse = function(response) {
    if (response.errorCode == 0) {
        window.aem.loginProvider=response.loginProvider;
    } else {
        window.logout();
    }
}

window.aem.parseQueryString=function(query) {
	  var vars = query.split("&");
	  var query_string = {};
	  for (var i = 0; i < vars.length; i++) {
		var pair = [vars[i].substr(0,vars[i].indexOf('=')),vars[i].substr(vars[i].indexOf('=')+1)];
		
		// If first entry with this name
		if (typeof query_string[pair[0]] === "undefined") {
		  query_string[pair[0]] = decodeURIComponent(pair[1]);
		  // If second entry with this name
		} else if (typeof query_string[pair[0]] === "string") {
		  var arr = [query_string[pair[0]], decodeURIComponent(pair[1])];
		  query_string[pair[0]] = arr;
		  // If third or later entry with this name
		} else {
		  query_string[pair[0]].push(decodeURIComponent(pair[1]));
		}
	  }
	  return query_string;

}

window.aem.showLegalConsentError = function() {
	
	if($(".legal-consent-error").length <= 0) {
		var firstrowLayout = $('form[id="gigya-profile-form"]').first(".gigya-row-layout");
		var html = "<div class='gigya-row-layout legal-consent-error'>"+CQ.I18n.get("acceptconsentmsg")+"</div>";
		$(html).insertBefore(firstrowLayout);
	}
	
	var isLegalAgreedElement = $('.gigya-screen-dialog input[name="data.maxim.islegalagreedinprofile"]');
	var isLegalAgreedElementParent = $('.gigya-screen-dialog input[name="data.maxim.islegalagreedinprofile"]').parent();
	if(typeof $(isLegalAgreedElement) != "undefined" && !$(isLegalAgreedElementParent).hasClass('gigya-consent-checkbox-error')) {
		$(isLegalAgreedElementParent).addClass('gigya-consent-checkbox-error');
	}
}

window.aem.removeLegalConsentError = function() {
	var isLegalAgreedElement = $('.gigya-screen-dialog input[name="data.maxim.islegalagreedinprofile"]');
	var isLegalAgreedElementParent = $('.gigya-screen-dialog input[name="data.maxim.islegalagreedinprofile"]').parent();
	if(typeof $(isLegalAgreedElement) != "undefined" && $(isLegalAgreedElementParent).hasClass('gigya-consent-checkbox-error')) {
		$(isLegalAgreedElementParent).removeClass('gigya-consent-checkbox-error');
	}
}

window.aem.afterScreenSetLoadEvent = function(event) {
	
	if(typeof event != "undefined" && event.screen == "gigya-complete-registration-screen") {
	
		var email = $('#gigya-complete-registration-screen input[name="profile.email"]').val();
		if(typeof email != "undefined" && email.indexOf("@maximintegrated.com") > 0) {
			var subscribeElement = $('#gigya-complete-registration-screen input[name="data.subscribe"]');
			var companyElement = $('#gigya-complete-registration-screen input[name="data.maxim.company"]');
			
			$(subscribeElement).prop("checked","checked");
			
			var companyName = $(companyElement).val();
			if(companyName == "") {
				$(companyElement).val("Maxim Integrated");				
			}
		}
		
		
		
		$('#gigya-complete-registration-screen select[name="data.maxim.country"]').on("change", function() {
			var countryCode = window.aem.getCountryCode($(this).val());

			if (countryCode != "") {
				if ($('#gigya-complete-registration-screen div.phone-prefix input').length > 0) {
					$('#gigya-complete-registration-screen input[name="data.maxim.countrycode"]').val(countryCode);
				}
			}
		});
		
		var currentLanguage = getUrlLanguage();
		if(currentLanguage == "ja") {
			$('#gigya-complete-registration-screen select[name="data.maxim.country"]').val("JP");
		} else if(currentLanguage == "zh") {
			$('#gigya-complete-registration-screen select[name="data.maxim.country"]').val("CN");
		} else {
			$('#gigya-complete-registration-screen select[name="data.maxim.country"]').val("US");
		}
		$('#gigya-complete-registration-screen select[name="data.maxim.country"]').change();
		
		
		
	}
}
window.aem.actionBeforeProfileScreenLoad = function(event) {

	if(typeof event != "undefined" && event.currentScreen == "gigya-update-profile-screen") {
		var islegalAgreed = CQ_Analytics.PersonalInfoStoreMgr.getProperty("islegalagreedinprofile");
			
		if(islegalAgreed != "" && islegalAgreed != "true") {
			aem.showLegalConsentError();
			$('.gigya-screen-dialog .gigya-screen-dialog-close').css("visibility","hidden");
			
			window.addEventListener("keyup", function (event) {
				event.stopPropagation();
			}, true);
		} else {
			aem.customGigyaPopupCloseEvent();
		}
		
		var modifyProfileCountElement = $('#gigya-profile-form input[name="data.maxim.profilemodifycount"]');
		
		if(typeof modifyProfileCountElement != "undefined") {
			var currentCount = "",updatedCount = 1;
			if(typeof event != "undefined" && typeof event.data != "undefined" && typeof event.data.maxim != "undefined" && typeof event.data.maxim.profilemodifycount != "undefined") {
				currentCount = event.data.maxim.profilemodifycount;
			}
			if(currentCount == "") {
				$(modifyProfileCountElement).val(updatedCount);
			} else {
				$(modifyProfileCountElement).val(parseInt(currentCount)+1);
			}
		}
		
		
		if (typeof aem.loginProvider != "undefined" && aem.loginProvider != "site") {
			if ($('.gigya-screen-dialog .gigya-profile-form').length > 0) {
				var resetPassword = $('.gigya-screen-dialog .gigya-profile-form').find("[data-switch-screen='gigya-change-password-screen']")
				if ($(resetPassword).length > 0) {
					$(resetPassword).css('display', 'none');
				}
			}
		}
		
		$('#gigya-profile-form input[name="data.maxim.islegalagreedinprofile"]').on("change", function() {
			
			if(this.checked == false) {
				aem.showLegalConsentError();
			} else {
				window.aem.removeLegalConsentError();
			}
		});
		
		$('#gigya-profile-form select[name="data.maxim.country"]').on("change", function() {
			var countryCode = window.aem.getCountryCode($(this).val());

			if (countryCode != "") {
				if ($('#gigya-profile-form div.phone-prefix input').length > 0) {
					$('#gigya-profile-form input[name="data.maxim.countrycode"]').val(countryCode);
				}
			}
		});
		$('#gigya-profile-form select[name="data.maxim.country"]').change();

		var engineer = [
			['--Select--', 'na'],
			['Design Engineer', 'design_engineer'],
			['Electrical Engineer', 'electrical_engineer'],
			['Hardware Engineer', 'hardware_engineer'],
			['Electronic Engineer', 'electronic_engineer']
		];
		var software = [
			['--Select--', 'na'],
			['Firmware Engineer', 'firmware_engineer'],
			['Application Engineer', 'application_engineer']
		];

		var ddl1 = $('#gigya-profile-form select[name="data.maxim.jobrole"]')[0];
		var ddl2 = $('#gigya-profile-form select[name="data.maxim.jobtitle"]')[0];

		$(ddl1).on("change", function() {
			switch (this.value) {
				case 'engineer_technical':
					$(ddl2).parent(".gigya-composite-control-dropdown").css("display","block");
					var currentValue = $(ddl2).val();
					ddl2.options.length = 0;
					for (i = 0; i < engineer.length; i++) {
						window.aem.createOption(ddl2, engineer[i][0], engineer[i][1]);
					}
					var isCorrectValue = engineer.filter(function (engineer) { return engineer[1] == currentValue }).length > 0 ? true : false;
					if(isCorrectValue) {
						$(ddl2).val(currentValue);
					} else {
						$(ddl2).val("na");
					}
					break;
				case 'software':
					$(ddl2).parent(".gigya-composite-control-dropdown").css("display","block");
					var currentValue = $(ddl2).val();
					ddl2.options.length = 0;
					for (i = 0; i < software.length; i++) {
						window.aem.createOption(ddl2, software[i][0], software[i][1]);
					}
					var isCorrectValue = software.filter(function (software) { return software[1] == currentValue }).length > 0 ? true : false;
					if(isCorrectValue) {
						$(ddl2).val(currentValue);
					} else {
						$(ddl2).val("na");
					}
					break;
				default:
					ddl2.value = "na";
					$(ddl2).parent(".gigya-composite-control-dropdown").css("display","none");
					break;

			}
		});

		$('#gigya-profile-form select[name="data.maxim.jobrole"]').change();
		
		if (CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) === true && CQ_Analytics.PersonalInfoStoreMgr.getProperty("email").indexOf('@maximintegrated.com') === -1) {
			
			/*var socialPluginGrid = $('#gigya-profile-form .gigya-layout-row')[2];
			$(socialPluginGrid).append("<div id ='social_mgt'></div>");
			var w_params = {
			version: 2,
			containerID: "social_mgt",
			width: "168",
			height: "50",
			requiredCapabilities: "login",
			enabledProviders: "facebook,linkedin,wechat",
			showTermsLink: "false",
			showEditLink: "true",
			hideGigyaLink: "true",
			customButtons:[]
		};
			gigya.socialize.showAddConnectionsUI(w_params);	*/	
			
		}
		else{
			$('.maximremovesection').remove();
		}
	
		var currentLanguage = getUrlLanguage();
		if(currentLanguage == "ja") {
			$('#gigya-update-profile-screen input[name="profile.locale"]').val("ja");
		} else if(currentLanguage == "zh") {
			$('#gigya-update-profile-screen input[name="profile.locale"]').val("zh-cn");
		} else {
			$('#gigya-update-profile-screen input[name="profile.locale"]').val("en");
		}	
	}
	
}

window.aem.createOption = function(ddl, text, value) {
    var opt = document.createElement('option');
    opt.value = value;
    opt.text = text;
    ddl.options.add(opt);
}

window.aem.gigyaProfileUpdate=function(response) {

    if (response.response.errorCode === 0) {

		if(typeof response.data.maxim.islegalagreedinprofile != "undefined" && response.data.maxim.islegalagreedinprofile == true) {
	        bootbox.alert(CQ.I18n.get("gigyaProfileSavedSuccessfully"));
		}else {
			window.logout();
			return;
		}
		
        var firstname = response.profile.firstName;
        var lastname = response.profile.lastName;
        var companyName = response.data.maxim.company;
        var roleName = response.data.maxim.jobrole;
        var roleTitle = response.data.maxim.jobtitle;
        var phoneNumber = response.data.maxim.phone;
        var country = response.data.maxim.country;
        var avatar = response.profile.photoURL;
		var isLegalAgreedInProfile = response.data.maxim.islegalagreedinprofile;
		
		

        var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");

        var formData = "authorizableId=" + encodeURIComponent(authorizableId);

        if (typeof firstname != "undefined" && firstname != "") {
            formData += "&" + encodeURIComponent("profile/givenName") + "=" + encodeURIComponent(firstname);
        }
        if (typeof lastname != "undefined" && lastname != "") {
            formData += "&" + encodeURIComponent("profile/familyName") + "=" + encodeURIComponent(lastname);
        }
        if (typeof companyName != "undefined" && companyName != "") {
            formData += "&" + encodeURIComponent("profile/company") + "=" + encodeURIComponent(companyName);
        }
        if (typeof roleName != "undefined" && roleName != "") {
            formData += "&" + encodeURIComponent("profile/role") + "=" + encodeURIComponent(roleName);
        }
        if (typeof roleTitle != "undefined" && roleTitle != "") {
            formData += "&" + encodeURIComponent("profile/roleTitle") + "=" + encodeURIComponent(roleTitle);
        }
        if (typeof phoneNumber != "undefined" && phoneNumber != "") {
            formData += "&" + encodeURIComponent("profile/phone") + "=" + encodeURIComponent(phoneNumber);
        }
        if (typeof country != "undefined" && country != "") {
            formData += "&" + encodeURIComponent("profile/country") + "=" + encodeURIComponent(country);
        }
        if (typeof avatar != "undefined" && avatar != "") {
            formData += "&" + encodeURIComponent("profile/avatar") + "=" + encodeURIComponent(avatar);
        }
		 if (typeof isLegalAgreedInProfile != "undefined" && isLegalAgreedInProfile != "") {
            formData += "&" + encodeURIComponent("profile/islegalagreedinprofile") + "=" + encodeURIComponent(isLegalAgreedInProfile);
        }



        jQuery.ajax({
            url: "/bin/AccountModificationServlet",
            type: "POST",
            contentType: "application/x-www-form-urlencoded",
            data: formData,
            success: function(data) {
                if (data.status) {
					var userProfileLoaderLink = "";
		
					if(typeof personalInfoLink != "undefined" && typeof personalInfoLink.url != "undefined") {
						userProfileLoaderLink = personalInfoLink.url;
					} else {
						userProfileLoaderLink = $('.personal-info-link').length > 0 ? $('.personal-info-link').data("personal-info-link") : "";
					}
					
					if(userProfileLoaderLink != "") {
						CQ_Analytics.PersonalInfoStoreMgr.init(userProfileLoaderLink, getProfileFieldValues);
					}
                   

                    if (CQ_Analytics.PersonalInfoStoreMgr.getProperty("avatar") != "") {
                        $('#myMaximLinkImg #mymi-icon').attr("src", CQ_Analytics.PersonalInfoStoreMgr.getProperty("avatar"));
                    }

                    var country = getUrlCountry();
					var userName = "";
                    if (country == 'en' || country == 'jp') {
						userName = CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName") + " " + CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName");
                        
                    } else {
						userName = CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName") + " " + CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName");
                    }
					$('.myMaximSpan').html(userName.length > 8 ? userName.slice(0,8)+"..." : userName).attr("style", "text-transform:capitalize");

                } else
                    alert(data.error);
            },
            error: function(jqXHR, exception) {
                if (jqXHR.status === 0) {
                    alert("Not connect. Verify Network.");
                } else if (jqXHR.status == 404) {
                    alert("Requested page not found. [404]");
                } else if (jqXHR.status == 500) {
                    alert("Internal Server Error [500].");
                } else if (exception === 'parsererror') {
                    alert("Requested JSON parse failed.");
                } else if (exception === 'timeout') {
                    alert("Time out error.");
                } else if (exception === 'abort') {
                    alert("Ajax request aborted.");
                } else {
                    alert("Uncaught Error: " + jqXHR.responseText);
                }
            }
        });
		
		
    }


}


window.aem.customGigyaPopupCloseEvent = function() {
	$('body #gigya-screen-dialog-page-overlay').click(function(event) {
				if ($(this).parent('.gigya-screen-dialog').length > -1) {
					$(this).parent('.gigya-screen-dialog').hide();
				}
				
				aem.hideSiteLoader();
	});
	
	

}

window.aem.sendResetMailIfRequired = function(e) {
	
	if(e.response) {
		var response = e.response;
		
		if(response.errorCode) {
			var errorNumber = response.errorCode;
			var requestParameters = window.aem.parseQueryString(location.search.substring(1));
			
			var apikey = requestParameters.apiKey;
			var uid = requestParameters.id;
			
			if(apikey != undefined && apikey != "" && uid != undefined && uid != "" && errorNumber == 403025) {
				 jQuery.ajax({
					url: "/bin/sendResetPasswordLink",
					type: "POST",
					data: "apiKey="+encodeURIComponent(apikey)+"&uid="+encodeURIComponent(uid),
					success: function(data) {
						if (data) {
							console.log(data);
						}
					},
					error: function(jqXHR, exception) {
						console.log("System error occured");
					}
				});
				
			}
		}
	}
		
}
window.aem.actionBeforeLoginScreenLoad = function() {
	window.aem.customGigyaPopupCloseEvent();
	
	var registerScreenSwitchElement = $('#gigya-login-form a[data-switch-screen = "gigya-register-screen"]');
	
	if(typeof registerScreenSwitchElement != undefined) {
		$(registerScreenSwitchElement).attr('data-switch-screen','');
		
		$(registerScreenSwitchElement).click(function() {
				aem.hideSiteLoader();
				if ($(this).parent('.gigya-screen-dialog').length > -1) {
					$(this).parent('.gigya-screen-dialog').hide();
				}
				$('.register-button').click();
		});
		
		//$(input['name="data.maxim.registerPageName"']).val(document.referer)
	}
}

window.aem.actionBeforeEmbedLoginScreenLoad = function() {
	var language = getUrlLanguage();
	
	if($('#embedContainer .embed-screen-reg-css').length > 0 ){	
		$('.embed-screen-reg-css').attr("data-switch-screen","");
        $('#embedContainer .embed-screen-reg-css').click(function() {
			trackRegistrationStart();
			
			if(typeof gigya != "undefined") {
				if(language == "zh") {
					gigya.accounts.showScreenSet(aem.embedRegisterForChina.params);
				} else {
					gigya.accounts.showScreenSet(aem.embedRegister.params);
				}
			}
        });
        
       // $(input['name="data.maxim.registerPageName"']).val(document.referer)
	}
}

window.aem.afterRegisterFormSubmit = function(object) {
	if(window.digitalDataSearch.form != undefined && object.screen == "gigya-register-screen") {
		window.digitalDataSearch.form.event = "form-complete";
		trackRegistrationEnd();
	}
}

window.aem.showRegisterScreenSet = function() {
	trackRegistrationStart();
	var language = getUrlLanguage();
		if(typeof gigya != "undefined") {
			if(language == "zh") {
				gigya.accounts.showScreenSet(aem.registerForChina.params);
			} else {
				gigya.accounts.showScreenSet(aem.register.params);
			}
		}
}

window.aem.gigyaRegisterScreen = function(formData, eventType, callback) {
	var errors = {};
	// Override default error message for required fields.
	if (formData['email'] === '') {
		errors['email'] = CQ.I18n.get("gigyaValidationsEmailRequired");
	}
	if (formData['email'] != '' && formData['email'].toLowerCase().endsWith("@maximintegrated.com")) {
		errors['email'] = CQ.I18n.get("gigyaValidationsInternalUser");
	}
	if (formData['profile.firstName'] === '') {
		errors['profile.firstName'] = CQ.I18n.get("gigyaValidationsFirstNameRequired");
	}
	// @, #, /, \ Not allowed in First Name
	/*var nameformat = /[@#\/\\]/;
    if (formData['profile.firstName'] != '' && formData['profile.firstName'].match(nameformat)) {
		errors['profile.firstName'] = CQ.I18n.get("gigyaValidationsFirstNameFormat");
	}*/
	if (formData['profile.lastName'] === '') {
		errors['profile.lastName'] = CQ.I18n.get("gigyaValidationsLastNameRequired");
	}
	/*// @, #, /, \ Not allowed in Last Name
	if (formData['profile.lastName'] != '' && formData['profile.lastName'].match(nameformat)) {
		errors['profile.lastName'] = CQ.I18n.get("gigyaValidationsFirstNameFormat");
	}
	// more than 1 period not allowed in Name (firstName, Lastname)
	var periodfirstname = formData['profile.firstName'];
    var periodlastname = formData['profile.lastName'];
    var combinedname = periodfirstname + periodlastname;
    if (combinedname.split(".").length >2) {
		errors['profile.firstName'] = CQ.I18n.get("gigyaValidationsCombinedNameFormat");
        errors['profile.lastName'] = CQ.I18n.get("gigyaValidationsCombinedNameFormat");
	}
    
    if (periodfirstname.trim().length < 2) {
		errors['profile.firstName'] = CQ.I18n.get("gigyaValidationsNameCharLength");
	}
    if (periodlastname.trim().length < 2) {
		errors['profile.lastName'] = CQ.I18n.get("gigyaValidationsNameCharLength");
	}*/
	
	if (formData['data.maxim.company'] === '') {
		errors['data.maxim.company'] = CQ.I18n.get("gigyaValidationsCompanyRequired");
	}
	
	 /*if (formData['data.maxim.company'].match(nameformat)) {
			errors['data.maxim.company'] = CQ.I18n.get("gigyaValidationsFirstNameFormat");
		}
    var periodCompany = formData['data.maxim.company'];
    if (periodCompany.split(".").length >2) {
		errors['data.maxim.company'] = CQ.I18n.get("gigyaValidationsCombinedNameFormat");
	}
    if (periodCompany.trim().length < 2) {
		errors['data.maxim.company'] = CQ.I18n.get("gigyaValidationsNameCharLength");
	}*/

	var phoneno = /^(?!0+$)[0-9][0-9]{6,15}$/
	if (formData['data.maxim.phone'] != '' && !formData['data.maxim.phone'].match(phoneno)) {
		errors['data.maxim.phone'] = CQ.I18n.get("gigyaValidationsPhoneNumberRestriction");
	}

	var extension = /^[0-9]{1,10}$/;
	if (formData['data.maxim.phoneextension'] != '' && !formData['data.maxim.phoneextension'].match(extension)) {
		errors['data.maxim.phoneextension'] = CQ.I18n.get("gigyaValidationsExtensionRestriction");
	}
	
	//var pssword = /^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[!@#$%^&*]).{6,}$/;
	var pssword = /(?=^.{6,}$)(?=.*\d)(?=.*[!@#$%^&*]+)(?![.\n])(?=.*[A-Z])(?=.*[a-z]).*$/;
	if (formData['password'] != '' && !formData['password'].match(pssword)) {
		errors['password'] = CQ.I18n.get("gigyaValidationsPasswordRestriction1")+CQ.I18n.get("gigyaValidationsPasswordRestriction2");
	}

	return errors;
}

window.aem.registrationNocaptcha = function(formData, eventType, callback) {
	var errors = {};

	var pssword = /(?=^.{6,}$)(?=.*\d)(?=.*[!@#$%^&*]+)(?![.\n])(?=.*[A-Z])(?=.*[a-z]).*$/;
	if (formData['password'] != '' && !formData['password'].match(pssword)) {
		errors['password'] = CQ.I18n.get("gigyaValidationsPasswordRestriction1")+CQ.I18n.get("gigyaValidationsPasswordRestriction2");
	}
	
	var phoneno = /^(?!0+$)[0-9][0-9]{6,15}$/;
	if (formData['data.maxim.phone'] != '' && !formData['data.maxim.phone'].match(phoneno)) {
		errors['data.maxim.phone'] = CQ.I18n.get("gigyaValidationsPhoneNumberRestriction");
	}

	return errors;
}

window.aem.gigyaLoginScreen = function(formData, eventType, callback) {
	var errors = {};

	if (formData['loginID'] === '') {
		errors['loginID'] = CQ.I18n.get("gigyaValidationsEmailRequired");
	}
	if (formData['loginID'] != '' && formData['loginID'].toLowerCase().endsWith("@maximintegrated.com")) {			
		errors['loginID'] = CQ.I18n.get("gigyaValidationsLoginInternalUser");
	}

	return errors;
}

