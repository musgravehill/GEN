if(typeof String.prototype.trim !== 'function') {
  String.prototype.trim = function() {
    return this.replace(/^\s+|\s+$/g, ''); 
  }
}

function getParameterByName(name) {
    name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
    var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
        results = regex.exec(location.search);
    return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
}

function validateEmail(email) { 
    var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);
} 
function containsOnlyAscii(str) {
    return  /^[\000-\177]*$/.test(str) ;
}

function cardDataHtml(jsonData,index)
{
	    var referenceGuide = "/content/maximintegrated/en/design/reference-design-center/system-board/"+jsonData.docID+".html/tb_tab2";
	    var schematicDiagram = "/content/maximintegrated/en/design/reference-design-center/system-board/"+jsonData.docID+".html/tb_tab2";
		var cardHtml ='	<div class="card"> '
			    +'<div class="card-header" id="referenceHead_'+index+'_'+selectedMap+'">'
			    +'    <h2 class="mb-0">'
			    +'       <button class="btn btn-link btn-block text-left collapsed" type="button"'
			     +'           data-toggle="collapse" data-target="#referenceCollapse_'+index+'_'+selectedMap+'"'
			      +'          aria-expanded="true" aria-controls="referenceCollapse_'+index+'_'+selectedMap+'">'
			      +'          <span><a href="'+jsonData.refrenceDesignPath+'.html">'+jsonData.part_number+'</a></span>'

			        +'    </button>'
			        +'</h2>'
			    +'</div>'
		
			    +'<div id="referenceCollapse_'+index+'_'+selectedMap+'" class="ref-expand collapse" aria-labelledby="referenceHead_'+index+'_'+selectedMap+'" data-parent="#referenceAccordion_'+selectedMap+'">'
			      +'  <div class="card-body">'
			      +'         <span>'+jsonData.title+'</span>'
			        +'    <div class="product-desc">'
			        +'        <div class="product-img"><img src="/etc/designs/maximintegrated/_images/design-diagram-reference-img.png"></div>'
			           +'     <ul style="display:none">'
			             +'       <li><i class="fa fa-file" aria-hidden="true"></i> <a href="#">Schematic/Block diagram</a></li>'
			               +'     <li><i class="fa fa-file" aria-hidden="true"></i> <a href="#">Reference guide</a></li>'
			                +'    <li><i class="fa fa-file" aria-hidden="true"></i> <a href="'+jsonData.refrenceDesignPath+'.html">View reference design</a></li>'
			               +' </ul>'
			       +'     </div>'
			     +'   </div>'
			  +'  </div>'
		    +'</div>';
	return cardHtml;
}

function createCompareURL(compareArrList){
       	var prodStr="";
        for(var i=0;i<compareArrList.length;i++){
            (prodStr != "") ? prodStr+=","+compareArrList[i] : prodStr+=compareArrList[i];
        }
		prodStr="&partNumbers="+prodStr;
		return prodStr;
    }

var compareArrayItems=[];
var compareFamilies=[];

function compareProducts(compareIndex){
	console.log("sdvs");
	var comparePopUp=false;
	var valueChanged=true;
	compareProdArray=compareArrayItems[compareIndex-1];
	
		//console.log("inisde");
        var compareParts=createCompareURL(compareProdArray);
		//console.log("outside");
        var location=window.location.href;
        if((location).indexOf("fam")!=-1) {
        	//familyName=getQueryParamValue("fam");
        } else {
        	familyName=compareFamilies[compareIndex-1];
        }
        var currentLanguage = document.documentElement.lang;
		//var disabledAttr = "";
		//var hideAttr = addHiddenattr();
		//disabledAttr = hideAttr.length > 0 ? "&disabledAttr="+hideAttr : '';
        var compareURL="/content/maximintegrated/"+currentLanguage+"/products/parametric/compare.html?fam="+familyName+compareParts+"&disabledAttr=886&locale="+currentLanguage;

		

        if(comparePopUp.closed) {
           valueChanged=true;
		}
        if(comparePopUp!=false && valueChanged) {
        	comparePopUp.close();
        }
        if(valueChanged) {
            comparePopUp =window.open(compareURL,"Compare Products","width=700,height=580,resizable=yes,scrollbars=yes",true);
        } else{
            comparePopUp.focus();
        }
        valueChanged=false;

}

function mappopwithhref(href, target, area){
	
	if(target=="_blank"){
        window.open(href);
    }
	if(area.id!=undefined)
	{
		var urlParams = window.location.search;
	    urlParams=urlParams.split("?")[1];
	    var queryParam = urlParams.split("&");
	    if(queryParam.length>1)
	    {
	    	urlParams = queryParam[0];
	    	urlParams +="&block="+area.id;
	    }
	    else if(queryParam.length == 1)
	    {
	    	urlParams +="&block="+area.id;
	    }
		history.replaceState(null,null,"?"+urlParams);
	}
   // window.open(href);
    $("#diagram-data_"+selectedMap+"").removeClass("d-none");
    $("#mainLinkedAccordion_"+selectedMap+"").removeClass("d-none");
    $("#mainProductAccordion_"+selectedMap+"").addClass("d-none");
    $("#mainReferenceAccordian_"+selectedMap+"").addClass("d-none");
    var title=area.title;
    var innerElements="<h4 class='mainLinkedAccordionHeader'><span>"+
        				title+
                        "</span><i class='fa fa-angle-right'></i>"+
                        "<a href="+href+" target="+target+">"+area.getAttribute("data-title")+"</a>"+
                        "</h4>";
    $("#mainLinkedAccordion_"+selectedMap+"").html(innerElements);
}



function mappop(mapname, mapnum, area)
{
	if(area.id!=undefined)
	{
		var urlParams = window.location.search;
	    urlParams=urlParams.split("?")[1];
	    var queryParam = urlParams.split("&");
	    if(queryParam.length>1)
	    {
	    	urlParams = queryParam[0];
	    	urlParams +="&block="+area.id;
	    }
	    else if(queryParam.length == 1)
	    {
	    	urlParams +="&block="+area.id;
	    }
		history.replaceState(null,null,"?"+urlParams);
	}
   
	var pathUrl = translatePath('/content/maximintegrated/'+getUrlLanguage()+'/appservice/productservice/_jcr_content.imagemap.json');
    $.ajax({
        url:pathUrl+"/"+mapname+"/"+mapnum,
        dataType:"json",
        type: 'get',
        xhrFields: {
        withCredentials: true
    },
   success:function(data){
	 
	   if(data.status)
        {
            var str="";
			var diagDesc="";
			
			var prodData="";
			var prodHead="";
			var prodChild="";
			var prodSubchild="";
			var finalProdChild="";
			var finalSubChild="";
			var finaldivChild="";
			var closingDivs="";
			var closingInnerDivs=  "</div>"
								+ "</div>"
							+ "</div>"
						+ "</div>";
			var compare_button="";
			//var compareProdArr=[];
			
			var cardData="";
            var itempartNumber="";
            var locallanguage="";
            if(getUrlLanguage()=="en")
            {
                locallanguage="en";
            }
           if(getUrlLanguage()=="ja")
            {
                locallanguage="jp";
            }
         if(getUrlLanguage()=="zh")
            {
                locallanguage="cn";
            }
            var lan="\""+"/"+locallanguage;
			var child=[];
            var subChild=[];
            var itemsList=[];
            var itemsCount=0;
            $.each(data.items, function(index, item){
            	if(item.mainfamily!="" && item.mainfamily!=null && item.mainfamily!=undefined && item.parentfamily!="" && item.parentfamily!=null && item.parentfamily!=undefined){
            		child.push(item.mainfamily);
            		subChild.push(item.parentfamily);
                    itemsList.push(item);
            		itemsCount++;
            	}
            });
            var countChild= countSequences(child);
            var countSubChild=countSequences(subChild);
            var childIndex=0;
            var compareid=0;
            var compareProdArr=[];
            var compareFamily="";
            compareArrayItems=[];
            compareFamilies=[];

			diagDesc = "<div class='diagram-description'>"
						+ "<div class='row'> "
							+ "<div class='col-sm-6' id='mainProductAccordion_"+selectedMap+"'>";
				
			prodHead =		      "<h4>Products ("+itemsCount+")</h4>"
									+ "<div class='accordion' id='productsAccordion_"+selectedMap+"'>";

			for (let key1 in countChild) {
			prodChild =					"<div class='card'>"
											+ "<div class='card-header' id='productHeadThree_"+selectedMap+"'>"
												+ "<h2 class='mb-0'>"
													+ "<button class='btn btn-link btn-block text-left' type='button' data-toggle='collapse' data-target='#productCollapseThree_"+selectedMap+"' aria-expanded='false' aria-controls='productCollapseThree_"+selectedMap+"'>"
														+ "<span>"+key1+" ("+countChild[key1]+")</span>"
													+ "</button>"
												+ "</h2>" 
											+ "</div>"
											+ "<div id='productCollapseThree_"+selectedMap+"' class='collapse show in' aria-labelledby='productHeadThree_"+selectedMap+"' data-parent='#productsAccordion_"+selectedMap+"'>"
												+ "<div class='card-body'>"
													+ "<div class='accordion second expand-last' id='productsSubAccordion_"+selectedMap+"'>";

			for (let key2 in countSubChild) {
                var i;
                var isMatch=false;
                for(i=0;i<itemsList.length;i++){
                    if(itemsList[i].mainfamily==key1 && itemsList[i].parentfamily==key2){
                        isMatch=true;
                    }
                }
            if(isMatch){
			var productType = key2.replace(/-/g, ' ');
			compare_button="";
			prodSubchild = 			        	 	"<div class='card expanded'>"
															+"<div class='card-header' id='productSubHead"+childIndex+"_"+selectedMap+"'>"
																+"<h2 class='mb-0'>"
																	+"<button class='btn btn-link compare_bt btn-block text-left collapsed' type='button' data-toggle='collapse' data-target='#productSubCollapse"+childIndex+"_"+selectedMap+"' aria-expanded='false' aria-controls='productSubCollapse"+childIndex+"_"+selectedMap+"'>"
																		+"<span>"+productType+" ("+countSubChild[key2]+")</span>"
																	+"</button>"
																+"</h2>";

			if(countSubChild[key2]>1){	
				compareProdArr=[];
				compareid++;
				$.each(data.items, function(index, item){
					if(item.parentfamily!=undefined && item.parentfamily!="" && item.parentfamily!=null && item.parentfamily==key2) {
						 itempartNumber=$.trim(item.partNumber);
						 compareProdArr.push(itempartNumber);
						 compareFamily=item.familyName;
					}
				});
				compareArrayItems.push(compareProdArr); 
				compareFamilies.push(compareFamily);
				
				compare_button="<button id='btn-compare"+compareid+"' type='button' onclick=\'compareProducts(\"" + compareid + "\")\' class='btn btn-primary acc-btn'>Compare</button>";
			}
			finaldivChild=  "</div>"
				+"<div id='productSubCollapse"+childIndex+"_"+selectedMap+"' class='collapse' aria-labelledby='productSubHead"+childIndex+"_"+selectedMap+"' data-parent='#productsSubAccordion_"+selectedMap+"'>"
			+"<div class='card-body'>"
			+"<div class='accordion third' id='productsAccordion"+childIndex+"_"+selectedMap+"'>";
			var cardData="";
			$.each(data.items, function(index, item){
	            if(item.parentfamily!=undefined && item.parentfamily!="" && item.parentfamily!=null && item.parentfamily==key2) {
			    itempartNumber=$.trim(item.partNumber);
                var productstr="href="+lan+"/products"; 
				 itempartNumber = itempartNumber.replace(/((href=\")(\/)(products))/gi,productstr);
				 itempartNumber = itempartNumber.replace(/((href=\")(products))/gi,productstr);
				 var displayImage = "/content/dam/images/products/product-images/default-chip.png";
				 if (item.pageImage !== undefined && item.pageImage!=null && item.pageImage !=''){
				 		displayImage = "//www.maximintegrated.com"+item.pageImage;
				 }

                str+="<tr><td><a href='"+decodeURIComponent(escape(item.url))+"'>"+itempartNumber+"</a></td><td>"
				+item.description+"</td><td>"+item.keyAdvantages+"</td></tr>";


				//compareProdArr.push(itempartNumber);

				cardData+="<div class='card'>"
							+ "<div class='card-header collapsed' id='productThirdHead"+index+"_"+selectedMap+"' data-toggle='collapse' data-target='#productThirdCollapse"+index+"_"+selectedMap+"' aria-expanded='true' aria-controls='productThirdCollapseOne_"+selectedMap+"' type='button'>"
							  + "<h2 class='mb-0'>"
					 			+ "<button class='btn btn-link btn-block text-left collapsed'> "
								  + "<span><a href='"+item.url+"'>"+itempartNumber+"</a></span>"
								+ "</button>"
							  + "</h2>"
							  + "<div class='card-body'> "
								+ "<p class='proddesc'>"+item.description+"</p>"
							  +"</div>"
					        + "</div>"
							+ "<div id='productThirdCollapse"+index+"_"+selectedMap+"' class='collapse' aria-labelledby='productThirdHead"+index+"_"+selectedMap+"' data-parent='#productsSubAccordion_"+selectedMap+"' aria-expanded='true' style=''> "
							  + "<div class='card-body'> "
								+ "<div class='product-desc'>"
								  + "<div class='product-img'><img src="+displayImage+"></div>"
								  + "<ul>"
									+ "<li>"
									//  + "<i class='fa fa-shopping-cart' aria-hidden='true'></i>"
									 + "<img src='/etc/designs/maximintegrated/_images/cart-icon.svg' class='cart-img-icon'/>"
									  + "<a href='"+item.url+"/#order-quality' class='textRed'>Order now</a>"
									+ "</li>"
									+ "<li>"
									  + "<i class='fa fa-file' aria-hidden='true'></i>"
									  + "<a href='"+item.url+"/tb_tab0#overview-tab'>Online data sheet</a>"
							 		+ "</li>"
								    + "<li>"
									  + "<i class='fa fa-file' aria-hidden='true'></i>"
									  + "<a href='https://datasheets.maximintegrated.com/en/ds/"+itempartNumber+".pdf'>Download data sheet</a>"
									+ "</li>"
								  + "</ul>"
							    + "</div>"
							  + "</div>"
						    + "</div>"
						  + "</div>";	
	            }});
			childIndex++;
			finalSubChild+=prodSubchild+compare_button+finaldivChild+cardData+closingInnerDivs;
                }
			}
			finalProdChild+=prodChild+finalSubChild+closingInnerDivs;
                finalSubChild="";
			}
													
			
									
			closingDivs = 											 "</div>"
							 									+ "</div>"
						    								+ "</div>"
						  								+ "</div>"
							  						+ "</div>"
						    					+ "</div>"
						  					+ "</div>"
							  			+ "</div>"
							  		+ "</div>";
            
             var startDiv = '<h4 class="reference-design-title">Reference Designs ('+data.referenceDesign.length+')</h4>'
                            +'<div class="accordion reference-accordion" id="referenceAccordion_'+selectedMap+'">'
                            
             var endDiv = ' </div> ';               
            
            var referenceDesignContent = '';
            
            $.each(data.referenceDesign, function(index, item){
            	referenceDesignContent +=cardDataHtml(item,index);
            });
            
          
            
            //hiding reference designs section if there are no references.
            var refBlockId = "mainReferenceAccordian_"+selectedMap+"";
            var prodBlockId = "mainProductAccordion_"+selectedMap+"";
           
			if(referenceDesignContent==""){
				$("#"+refBlockId+"").hide();
				//referenceDesignContent= "<span style='color: #6C6F70;padding-left: 10px;'>No Results present for Reference Designs</span>";
			}else {
				$("#"+refBlockId+"").show();
			}
            
            var refDesignData = startDiv+referenceDesignContent+endDiv;

			prodData = prodHead + finalProdChild + closingDivs;
			
			//$('#productsThirdAccordion').html(cardData);
			$('#'+prodBlockId+'').html(prodData);
			$('#'+refBlockId+'').html(refDesignData);
            $('#mapholder').html(str);

            if(area!=undefined && area!=null)
             {
            	$('#maptitle_'+selectedMap+'').text(area.title);
            	var areaId = area.id;    
            	$("#blockdata_"+selectedMap+"").removeAttr("selected");
            //	$("#blockdata_"+selectedMap+" option[value='"+areaId+"']").attr("selected","selected");
            	$("#blockdata_"+selectedMap+"").val(areaId);
            	
              }
            else {
            	$('#maptitle_'+selectedMap+'').html("&nbsp;");
            }
            
            var redesign_map = $("#redesign_map_"+selectedMap+"");
            if(redesign_map!=undefined)
            	{
            		$('#modalmap').modal('hide');
            	}
            else
            	{
            	 	$('#modalmap').modal('show');
            	} 
            //Making first item in the reference design section as expanded
            if($('.reference-accordion div').first().closest('.card-header').find('.text-left')){
            	$('.reference-accordion div').first().closest('.card').find('.ref-expand').addClass('in');
            	$('.reference-accordion div').first().closest('.card').find('.card-header h2 .text-left').removeClass('collapsed');
            }
           $("#mainProductAccordion_"+selectedMap+" h4").addClass("mb-0");
			$("#diagram-img_"+selectedMap+"").removeClass("col-auto");
			$("#diagram-img_"+selectedMap+"").addClass("col-md-7");			
			$("#diagram-data_"+selectedMap+"").removeClass("d-none");

			$("#mainLinkedAccordion_"+selectedMap+"").addClass("d-none");
    		$("#mainProductAccordion_"+selectedMap+"").removeClass("d-none");
    		$("#mainReferenceAccordian_"+selectedMap+"").removeClass("d-none");

			var mobileCheck = /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i
			if(!mobileCheck.test(navigator.userAgent) ) {
				setTimeout(function(){
					var eee = $('#diagram_'+selectedMap+'').width();
					$('img[usemap]').mapster('resize', eee, 0);
					
					
					 
				},100);
			}
			
			$('map').imageMapResize();
        }
        else
            alert(data.error);
		}
	});

}

function getComboData(id)
{
	if(id==selectedMap)
	{
		var redesign_map = $("#redesign_map_"+selectedMap+"");
	//	 $("#redesign_map_"+selectedMap+"").remove(); 
		var blockdataDropDown = $("#blockdata_"+selectedMap+"");
		blockdataDropDown.empty();

		$("#redesign_map_"+selectedMap+"").find('area').each(function() { // This doesn't work because the .div_item children aren't populated?
			var mapfunc = $(this).attr("onclick");
			var areaId = $(this).attr("id");
			var areaTitle = $(this).attr("title");
			
			var param1 = "";
			var param2 = "";
			if(mapfunc!=undefined)
			{
				var funcParam = mapfunc.substring(mapfunc.indexOf("(")+1,mapfunc.indexOf(")"));
				var paramData = funcParam.split(",");
				param1 = paramData[0].replace(/["']/g, "");
				param2 = paramData[1].replace(/["']/g, "");
				
				
				if(areaId!=undefined && areaTitle!=undefined)
				{
					blockdataDropDown.append($('<option></option>').attr('value', areaId).text(areaTitle).attr("data-sname",param1).attr("data-bname",param2));
				}
				
			}else{
				if(areaId!=undefined && areaTitle!=undefined )
				{
					blockdataDropDown.append($('<option></option>').attr('value',areaId).text(areaTitle).attr("data-sname",param1).attr("data-bname",param2));
				}
			
               }
			
			
	     });
	}
}

function blockDropDownChange(selectedIndex,dropDrownOpt)
{
	var selected = $("#"+dropDrownOpt.id+"").find('option:selected');
	var sName = selected.data("sname");
	var bName = selected.data("bname");
	var areaId = selected.val();
	var areaTitle = selected.text();
	var area = {id:areaId,title:areaTitle};
	var object = selected.data("object");
        if(sName !="" && bName !=""){
            mappop(sName,bName,area);
        }
	
}

function translatePath(path)
{
    if(document.URL.indexOf("/content/maximintegrated")>0)
    {
        return path;
    }
    else
    {
        return path.replace("/content/maximintegrated/"+getUrlLanguage(),"/"+getUrlCountry());
    }
}

function parseHtmlEntities(str) {
    if(str != "undefined" && str != "" && str != null && str != "null" && str != undefined) {
    return str.replace(/&#([0-9]{1,4});/gi, function(match, numStr) {
        var num = parseInt(numStr, 10); // read num as normal number
        return String.fromCharCode(num);
    });
    }
}



function sortOnKeys(dict) {

    var sorted = [];
    for(var key in dict) {
        sorted[sorted.length] = key;
    }
    sorted.sort();

    var tempDict = {};
    for(var i = 0; i < sorted.length; i++) {
        tempDict[sorted[i]] = dict[sorted[i]];
    }

    return tempDict;
}



/*
	Handlebars helper functions for Catalog/Storefront search page 
*/


Handlebars.registerHelper('titleEncode', function(v1, options) {
    if(v1 != "" && v1 != "undefined" && v1 != undefined){
        return parseHtmlEntities(v1);
    }

});

Handlebars.registerHelper('decodeValue', function(v1, options) {
    if(v1 != "" && v1 != "undefined" && v1 != undefined){
        return decodeURIComponent(v1.replace(/\+/g, ' '));
    }

});

Handlebars.registerHelper('encodeuri', function(v1, options) {
    if(v1 != "" && v1 != "undefined" && v1 != undefined){
        return encodeURIComponent(v1);
    }

});

Handlebars.registerHelper('createurl', function(v1,v2, options) {
    if(v1 != "" && v1 != "undefined" && v1 != undefined && v2 != "" && v2 != "undefined" && v2 != undefined){
        return v1+encodeURIComponent(v2);
    }

});

Handlebars.registerHelper('trimHash', function(v1, options) {
    if(v1 != "" && v1 != "undefined" && v1 != undefined){
        key = v1.replace("#", "");
        return key;
    }

});

Handlebars.registerHelper('moqCheck', function(v1, v2, options) {
    var userType = CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType");
    if(userType != "DISTRIBUTOR" && v1 != "" && v1 != "undefined" && v1 != undefined){
        return v2;
    } else {
        return v1;
    }

});

Handlebars.registerHelper('packageEncode', function(v1, v2, v3, v4, v5, v6, options) {
    if(v1 != "" && v1 != null && v1 != "undefined" && v1 != undefined && v3 != "0" && v4 != "0" && v5 != "0" && v6 != "0"){
        return parseHtmlEntities(v1)+";";
    } else if(v2 != "" && v2 != "undefined" && v2 != undefined && v3 != "0" && v4 != "0" && v5 != "0" && v6 != "0"){
        return parseHtmlEntities(v2)+";";
    } else {
        return ""
    }

});

Handlebars.registerHelper('PIN', function(v1, options) {
    if(v1 != "" && v1 != "undefined"  && v1 != undefined && v1 != "0"){
        return v1+"Pin;";
    } else {
        return ""
    }

});

Handlebars.registerHelper('AREA', function(v1, v2, v3, options) {

    //console.log("pkg_length : "+ v1);
    //console.log("pkg_width : "+ v2);
    //console.log("pkg_height : "+ v3);

    if((v1 != "" && v1 != "undefined" && v1 != undefined && v1 != "0") && (v2 != "" && v2 != "undefined" && v2 != undefined && v2 != "0") && (v3 != "" && v3 != "undefined" && v3 != undefined && v3 != "0")){
        return v1+"mm(L) x " + v2 + "mm(W) x "+ v3 + "mm(H)";
    } else {
        return "";
    }

});

Handlebars.registerHelper('leadTime', function(v1, options) {
    if(v1 != ""  && parseInt(v1) > 0 && parseInt(v1) < 98 ){
		if(v1 > 16) {
			return "16+"+ Granite.I18n.get("daysText");
		} else {
			return v1+" "+ Granite.I18n.get("daysText");
		}
        
    } else {
        return Granite.I18n.get("contactFactory");
    }

});

Handlebars.registerHelper('getPartObject', function(context, options) {
    var cells = [], html, k;
    for (k in context) {
        if (context.hasOwnProperty(k)) {
            html = options.fn({
                key: k,
                value: context[k]
            }); 
            cells.push(html);
        }
    }
    return cells.join('');
});



Handlebars.registerHelper('revenueCart', function(v1, options) {
  if(v1 == "Cart" && (v1 != "" && v1 != 0)) {
    return options.fn(this);
  }
    return options.inverse(this);
});


Handlebars.registerHelper('priceBreak', function(v1,v2,v3, options) {
  if(v1 != "DISTRIBUTOR" && (v1 != "" && v1 != 0) && (v2 != "true" || (v2 == "true" && v3=="Tape")) && (v2 != "" && v2 != 0)&& (v3 != "" && v3 != 0)) {
    return options.fn(this);
  }
    return options.inverse(this);
});

Handlebars.registerHelper('sotParts', function(v2,v3, options) {
  if((v2 == "true" && v3=="Tape") && (v2 != "" && v2 != 0)&& (v3 != "" && v3 != 0)) {
	  return options.inverse(this);
  }
    return options.fn(this);
});

Handlebars.registerHelper('1000KPrice', function(v1,options) {
  if(v1 != "" && v1 != 0 && v1 != "No Volume Price" && v1 != "undefined") {
    var priceBreak = jQuery.parseJSON(v1);
    return priceBreak[priceBreak.length-1].formattedValue;
  }
});



Handlebars.registerHelper('ProductStatus', function(v1,options) {
	var statusJson = {
		"DEVELOPMENT" : "Preliminary",
		"PRE-RELEASE" : "Preliminary",
		"PRODUCTION" : "Active",
		"MATURE"	: "NRND",
		"LAST TIME BUY" : "Last Time Buy",
		"HI VOLUME ONLY" : "Active",
		
		"OBSOLETE": "NLA"
	};
  if(v1 != "" && v1 != "undefined") {    
	return statusJson[v1];
  }
});

Handlebars.registerHelper('carrierType', function(v1,options) {	
  if(v1 != "" && v1 != "undefined") {    
	return v1;
  } else {
	  return "N/A";
  }
});

Handlebars.registerHelper('currencyFormat', function(v1, options) {
    return accounting.formatMoney(v1);
});

Handlebars.registerHelper('totalWODeliveryCost', function(v1,v2, options) {
    var subTotal = v1;
    console.log(v1+"   "+v2+"  "+subTotal);
    if(v2 != "" && v2 != 0 && v2 != "undefined" && v2 != undefined){
    subTotal = parseInt(v1)-parseInt(v2);
    console.log("sub"+parseInt(subTotal));    
    }
    return accounting.formatMoney(subTotal);
});


Handlebars.registerHelper('currencyFormatDecimal', function(v1, options) {
    return accounting.formatMoney(v1,"$", 3);
});

Handlebars.registerHelper('currencyFormatPriceUnit', function(v1, v2, options) {
    var unitPrice = v1/v2;
    return accounting.formatMoney(unitPrice, "$", 3);
})

Handlebars.registerHelper('sampleCart', function(v1, options) {
 if(v1 == "SampleCart" && (v1 != "" && v1 != 0)) {
	return options.fn(this);
  }
  return options.inverse(this);
});


Handlebars.registerHelper('ifCond', function(v1, v2, options) {
  if(v2 == "PIN" && (v1 != "" && v1 != 0)) {
    return options.fn(this);
  }
  if(v2 == "SortKey" && (v1 != "" && v1 != 0) && v1 != "sortbyRoot") {
    return options.fn(this);
  }
  if(v2 == "AREA" && (v1 != "" && v1 != 0)) {
	return options.fn(this);
  }
  if(v2 == "PACKAGECODE" && (v1 != "" && v1 != 0 && !v1.match("EVKIT"))) {
	return options.fn(this);
  }  
   if(v2 == "DATASHEET" && (v1 != "" && v1 != 0)) {
	return options.fn(this);
  }
  if(v2 == "LINE2" && (v1 != "" && v1 != "NA")) {
	return options.fn(this);
  }
  if(v2 == "VAT" && (v1 != "" && v1 != "N/A" && v1 != "undefined" && v1 != undefined && v1 != "--")) {
	return options.fn(this);
  }
  if(v2 == "TYPE" && (v1 != "" && v1 != 0)) {
	return options.fn(this);
  }
  if(v2 == "NCNR" && (v1 != "" && v1 != 0 && v1 == "true")) {
        return options.fn(this);
    }
  if(v2 == "ProductLifeCycle" &&  (v1 != "" && v1 != 0) && (v1 != "OBSOLETE" && v1 != "DEVELOPMENT" && v1 != "PRE-RELEASE" ) && (v2 != "" && v2 != 0)) {
        return options.fn(this);
  }
  if(v2 == "USERTYPE" &&  (v1 != "" && v1 != 0) && (v1 == "DISTRIBUTOR" ) && (v2 != "" && v2 != 0)) {
        return options.fn(this);
  }
  if(v2 == v1) {
        return options.fn(this);
  }
  if(v2 == "ObsoleteCheck" && v1 == "OBSOLETE"){
      return options.fn(this);
  }
  if(v2 == "TAXId" && v1 == "KR"){
      return options.fn(this);
  }
  if(v2 == "NotTaxId" && v1 != "KR"){
      return options.fn(this);
  }
  if(v2 == "CANORDER" &&  (v1 != "" && v1 != 0) && (v1 == "true" ) && (v2 != "" && v2 != 0)) {
        return options.fn(this);
  }
  return options.inverse(this);
});


Handlebars.registerHelper('each-reverse', function(context) {
    var options = arguments[arguments.length - 1];
    var ret = '';

    if (context && context.length > 0) {
        for (var i = context.length - 1; i >= 0; i--) {
            ret += options.fn(context[i]);
        }
    } else {
        ret = options.inverse(this);
    }

    return ret;
});

//Helper function for <!--pricing--> avaliability 
Handlebars.registerHelper('pricing', function(context) {
    if (context && context.length > 0) {
    return context[0].formattedDate;
    }
});

Handlebars.registerHelper('pricingcheck', function(context, options) {
    if(context && context.length > 0){
        return options.fn(this);
    }      
    else{
        return options.inverse(this);
    }        
});

Handlebars.registerHelper('ifdistiAvaliability', function(context, options) {
    if(('mouser' in context)|| ('digiKey' in context) || ('farnell' in context))
        {
            if (('stock' in context.mouser) || ('stockOnHand' in context.digiKey) || ('stockValue' in context.farnell)){
                return options.fn(this);
            }
    else{
        return options.inverse(this);
    }
        }
    else{
        return options.inverse(this);
    }
        
});

Handlebars.registerHelper('noDistiAvaliability', function(context, options) {
	if(('mouser' in context)|| ('digiKey' in context) || ('farnell' in context))
        {
            if (('stock' in context.mouser) || ('stockOnHand' in context.digiKey) || ('stockValue' in context.farnell)){
                
				return options.inverse(this);
            }
    else{
        return options.fn(this);
    }
        }
    else{
        return options.fn(this);
    }
        
});

//Helper function for more stock avaliability
Handlebars.registerHelper('stockAvaliability', function(context){    
    var htmlTag = '';
    
    for(var item in context){
        htmlTag = htmlTag + '<tr><td>' + context[item].stockValue + '</td><td>' +  context[item].formattedDate + '</td></tr>';        
    }
    
    return htmlTag;

});


//Helper for topmark
Handlebars.registerHelper('topmarkcode', function(topmarkCode, url){    
    var htmlTag = '';
    console.log(url);
    if(topmarkCode == "Contact Technical Support"){
    	if(url != '')
    		url = 'https://maximsupport.microsoftcrmportals.com/en-US/support-center/';
        htmlTag = htmlTag + '<td class="tablebody3"><a target="_blank" href="'+$("#searchresults").data('contact-url')+'">'+topmarkCode+'</a></td>';       
    } else {
    	htmlTag = '<td class="tablebody3">'+topmarkCode+'</td>'
    }
    
    return htmlTag;

});

function getNextWeekMonday(date) {
    var d = date;
    var diff = d.getDate() - d.getDay() + 1;
    if (d.getDay() == 0)
        diff -= 7;
    diff += 7; // ugly hack to get next monday instead of current one
    return new Date(d.setDate(diff));
};

function getNextWeekFriday(date) {
    var d = this.getNextWeekMonday(date);
    return new Date(d.setDate(d.getDate() + 4));
};
/*
	Auto populate country/State functions for Profile/Storefront pages. 
*/
function sortResults(arr, prop, asc) {
    arr = arr.sort(function(a, b) {
        if (asc) return (a[prop] > b[prop]) ? 1 : ((a[prop] < b[prop]) ? -1 : 0);
        else return (b[prop] > a[prop]) ? 1 : ((b[prop] < a[prop]) ? -1 : 0);
    });
    return arr;
}

function populateCountry(countryDivId, countryFieldId, stateDivId, stateFieldId) {
    $.ajax({
                    type: "post",
                    url:'/bin/address',
                    data: "input=getCountry",
                    success: function(msg){ 
                        var json = jQuery.parseJSON(msg);
                        var arr = sortResults(json.country, "name", "asc");
                        
                        var html = '<label for="addressCountry" class="smallest">'+Granite.I18n.get("country")+'<font color="red"><b>*</b></font></label><select id="'+countryFieldId+'" onchange="populateState(&quot;'+stateDivId+'&quot;,&quot;'+stateFieldId+'&quot;,&quot;'+countryFieldId+'&quot;)" name="'+countryFieldId+'" class="form-control required" aria-required="true"><option  selected></option>';
                        for(var i=0;i<arr.length;i++){
                            var obj = arr[i];
                            //alert(obj.countryIsoCode+"\n"); Made US default.
                            if(obj.countryIsoCode == "US") { 
                                html = html + '<option class="form-control" selected value="'+obj.countryIsoCode+'" data-taxIdRequired="'+obj.taxIdRequired+'">'+obj.name+'</option>';
                            } else {
                            	html = html + '<option class="form-control" value="'+obj.countryIsoCode+'" data-taxIdRequired="'+obj.taxIdRequired+'">'+obj.name+'</option>';
                            }

                            }

                        html = html+'</select>';

                        
                        $("#"+countryDivId).html(html);

                    }
        }); 
}

function populateState(stateDivId, stateFieldId, preSelectedCountryId) {
    var selectedCountry = $('#'+preSelectedCountryId+' option:selected');
    var status = selectedCountry.data('taxidrequired'); 
    if(status==true){
        $('#vat').show();
    }
    var ccode = $('#'+preSelectedCountryId+' option:selected').val();
    if(preSelectedCountryId == "US") {
		ccode = "US";
    }
    $.ajax({
        type: "POST",
        url: '/bin/MiscUtilsServlet',
        data: "cmd=region&ccode=" + ccode,
        dataType: "json",
        success: function(msgI) {
        $("#"+stateDivId).show();
        var ht = '<label for="addressState" class="smallest">'+Granite.I18n.get("stateText")+'<font color="red"><b>*</b></font></label><br/>';
            if (msgI.items) {
                ht = ht + "<select class=\"form-control\" name='"+stateFieldId+"' id='"+stateFieldId+"'>";
                for (var rowIndex = 0; rowIndex < msgI.items.length; rowIndex++) {
                    var code = msgI.items[rowIndex].value;
                    ht = ht + '<option class="form-control" value="'+ccode+"-"+code + '" class="form-control">' + msgI.items[rowIndex].label + '</option>';
                }
                ht = ht + "<\select>";
           } 
           if(msgI.items != undefined){$("#"+stateDivId).html(ht); $("#"+stateDivId).html(ht);}
    else{	$("#"+stateDivId).html(ht); $("#"+stateDivId).hide();}

        },
            error: function(jqXHR, textStatus, errorThrown){
                $("#"+stateDivId).empty();
				$("#"+stateDivId).hide();
                }

    });
}



