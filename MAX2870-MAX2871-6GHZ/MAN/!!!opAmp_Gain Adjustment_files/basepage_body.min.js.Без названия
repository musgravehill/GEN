}

function openEmployeePortal(){
	
	if(!window.location.origin.includes("www.maximintegrated.com")){
        currentUrl = "https://maximemployeeqa.microsoftcrmportals.com/";
	}
	else{
        currentUrl = "https://maximemployeeportal.microsoftcrmportals.com";
	}
	window.open(currentUrl, '_blank');
	
}

function isEmpty(str) {
    return (!str || 0 === str.length);
}

function toggleProfileEndCustomer() {
	if ($('#profileEndCustomer').is(':visible')) {
		$('#profileEndCustomer').slideUp();
	}
	else {
		$('#profileEndCustomer').slideDown();
	}
}
function initQuoteCustomerInfo() {
	$("#myAuthorizableId").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId"));
	$("#myName").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName")+' '+CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName"));
	$("#myCompany").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("company",true));
	$("#myCity").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("city"));
	$("#profileEndCustomerCity").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("city"));
	$("#myZip").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("postalCode"));
	$("#profileEndCustomerZip").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("postalCode"));
	var state = (CQ_Analytics.PersonalInfoStoreMgr.getProperty("state") != "") ? CQ_Analytics.PersonalInfoStoreMgr.getProperty("state") : CQ_Analytics.PersonalInfoStoreMgr.getProperty("intState");
	$("#myState").val(state);
	$("#profileEndCustomerState").val(state);
	$("#myCountry").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("country"));
	$("#profileEndCustomerCountry").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("country"));
	$("#myEmail").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("email"));
	$("#myPhone").val(CQ_Analytics.PersonalInfoStoreMgr.getProperty("phone"));
	if (isEmpty(CQ_Analytics.PersonalInfoStoreMgr.getProperty("country")) || isEmpty(CQ_Analytics.PersonalInfoStoreMgr.getProperty("city")) || isEmpty(state)) {
		$("#userDetails").html('<p>'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName")+' '+CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName")+
				'<br>'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("email")+
				'<br>'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("phone")+
				'<br><div id="profileUserDetails"><p><span class="smallest"><i>Address information is missing. Please fill out your address completely.</i></span><a id="profileUserAddressDetails" class="uppercase smallest bold" href="#" onClick = "toggleProfileEndCustomer();"><br><span class="glyphicon glyphicon-plus" aria-hidden="true"></span>Edit address</a></p></div>');
		//$('#profileEndCustomer').slideDown();
	}
	else {
		$("#userDetails").html('<p>'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName")+' '+CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName")+'<br>\
				'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("email")+'<br>\
				'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("phone")+'<br>\
				'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("company",true)+'<br>\
				'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("city")+', '+ state +', ' +CQ_Analytics.PersonalInfoStoreMgr.getProperty("postalCode")+'<br>\
				'+CQ_Analytics.PersonalInfoStoreMgr.getProperty("country")+'</p>');
		//$('#profileEndCustomer').slideUp();
	}
}


function adjustRTIFrame()
{
	var containerWidth = $('#reqeustTrackerHolder .modal-content').width()-40;
    $('#rtIframe').attr('width', ""+containerWidth);
}
function supportByEmailInModal()
{
    var refreshIntervalId= setInterval(function() {
			if(document.readyState === "complete") {
    loadSaleforceForm('https://maximsupport.microsoftcrmportals.com/en-US/support/create-case/');
				clearInterval(refreshIntervalId);
}
		}, 3000);
}
function loadSaleforceForm(forceFormUrl)
{
    if(typeof forceFormUrl != "undefined" && forceFormUrl != "") {
        var win = window.open(forceFormUrl, '_blank');
		if(typeof win != "undefined" && win != null) {
  		win.focus();
  	} else {
		bootbox.alert(CQ.I18n.get("popupBlocked"));
	}
}
}

$(function(){
	var toLoadForm=false;
	//var formNewUrl='http://maximintegratedsupport.force.com';
	var formUrl = 'https://maximsupport.microsoftcrmportals.com';
	
		
	if(document.URL.indexOf("/TechSupportForm")>0)
    {
    	if(document.URL.indexOf("/TechSupportFormChina")>0)
    		formUrl="https://maximsupport.microsoftcrmportals.com/zh-CN/support-center/";
    	else if(document.URL.indexOf("/TechSupportFormJapan")>0)
    		formUrl="https://maximsupport.microsoftcrmportals.com/ja-JP/support-center/";
    	else
    		formUrl="https://maximsupport.microsoftcrmportals.com/en-US/support-center/";
    	toLoadForm=true;
    }else if(document.URL.indexOf("/SupportForm")>0)
    {
    	if(document.URL.indexOf("/SupportFormChina")>0)
    		formUrl="https://maximsupport.microsoftcrmportals.com/zh-CN/support-center/";
    	else if(document.URL.indexOf("/SupportFormJapan")>0)
    		formUrl="https://maximsupport.microsoftcrmportals.com/ja-JP/support-center/";
    	else
    		formUrl= "https://maximsupport.microsoftcrmportals.com/QA";
    	toLoadForm=true;
    }else if(document.URL.indexOf("/NDARequest")>0)
    {
    	var formUrl = 'https://maximsupport.microsoftcrmportals.com';
    	if(document.URL.indexOf("/NDARequestUSBProtectionProduct")>0)
        	formUrl+="/ndarequestusbprotectionproduct";
    	else if(document.URL.indexOf("/NDARequestMicrocontrollers")>0)
        	formUrl+="/ndarequestmicrocontrollers";
    	else
    		formUrl+="/nda";
		toLoadForm=true;
    }
	else{
		if(document.URL.indexOf(".com/support")>0 || document.URL.indexOf(".html/support")>0)
    {
		    formUrl= "https://maximsupport.microsoftcrmportals.com/en-US/support-center/";
    	    toLoadForm=true;
	}
	}
    if(toLoadForm)
    {
	    if (document.URL.lastIndexOf("?") >= 0) {
	        var queryString = document.URL.substring(document.URL.lastIndexOf("?")+1);
	        formUrl+="?"+queryString;
	    }
	    var refreshIntervalId= setInterval(function() {
			if(document.readyState === "complete") {
	    loadSaleforceForm(formUrl);
				clearInterval(refreshIntervalId);
    }
		}, 3000);
    }
});


var navBarProfileCallback = function()
{
	if(CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true)=="true" || CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true)==true)
    {
        login.status=true;
        //var s=s_gi("maxintegrateddefault");
        //s.visitorID=CQ_Analytics.PersonalInfoStoreMgr.getProperty("memberId");
        var visitor;
        if (typeof Visitor!=='undefined') {
            visitor = Visitor.getInstance("8D7A9CD35437E9A30A4C98A7@AdobeOrg");
            visitor.setCustomerIDs({
                 "userid":{
                     "id":CQ_Analytics.PersonalInfoStoreMgr.getProperty("memberId"),
                     "authState":1
                 }
            });
        }
    	updateVisitorId();
        var country=getUrlCountry();
        var userName = "";
        if (country == 'en' || country == 'jp') {
			userName = CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName") + " " + CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName");
        } else {
			userName = CQ_Analytics.PersonalInfoStoreMgr.getProperty("familyName") + " " + CQ_Analytics.PersonalInfoStoreMgr.getProperty("givenName");
        }
		$('.myMaximSpan').html(userName.length > 8 ? userName.slice(0,8)+"..." : userName).attr("style", "text-transform:capitalize");
        $('.myMaximLink').removeClass('disablelink');
        $('#myMaximLinkImg').removeClass('disablelink');
        $('.myMaximLogoutDiv').show();
        $('.myMaximLoginDiv').hide();
        $('#myMaximLogoutDl').removeAttr("style");
        $('#myMaximLoginDl').hide();
        $('#mymi-icon').attr("src",CQ_Analytics.PersonalInfoStoreMgr.getValue("avatar"));
        $('#mymi-icon-mobile').attr("src",CQ_Analytics.PersonalInfoStoreMgr.getValue("avatar")).show();
        initBookmark();
        setMaximConfidentialLink();
        initMyMaxim();
        initMyMaximBulletin();
        // checkTermsAccept();
        initQuoteCustomerInfo();
        netTermsStatusUpdate()

//        if("true"==CQ_Analytics.PersonalInfoStoreMgr.getProperty("softlaunch"))
        {
	        var requestTrackerName="Support Portal";
			var kbTrackerName="Knowledge Base Search";
	        var leftLinks = "";
			if($('#leftmenu').length > 0) {
				leftLinks=$('.nav-menu').find('a');
			} else if($('.nav-submenu-title-container').length > 0) {
				leftLinks=$('.nav-submenu-title-container').find('a');
			}
			
			var userType = CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType");

	        $.each( leftLinks, function(index, value) {

	        	if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/support/overview.html')>0 && userType!="EMPLOYEE")
	            {
	                $(value).attr('href','javascript:openSamlResponse()');
					$(value).attr('target','');
					$(value).find('div').text(requestTrackerName);
	                return;
	            }
	        	
	        	//Added
	        	if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/support/overview.html')>0 && userType=="EMPLOYEE"){
	        		
	        		 	$(value).attr('href','javascript:openEmployeePortal()');
						$(value).attr('target','');
						$(value).find('div').text(requestTrackerName);
		                return;
	        		
	        	}
				if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/knowledgebase/')>0)
	            {
	                if($(value).attr('href').indexOf('/zh-CN/')>0){
						kbTrackerName = "从知识库搜索答案";
					}
					if($(value).attr('href').indexOf('/ja-JP/')>0){
						kbTrackerName = "ナレッジベース検索";
					}
					$(value).attr('href','javascript:kbSamlResponse()');
					$(value).attr('target','');
					$(value).find('div').text(kbTrackerName);
	                return;
	            }
	        });
            var mobileLeftLinks = $('.navbar-nav').find('a');
	        $.each( mobileLeftLinks, function(index, value) {
	        	if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/support-center/')>0 && userType!="EMPLOYEE")
	            {
	                $(value).attr('href','javascript:openSamlResponse()');
					$(value).text(requestTrackerName);
	                return;
	            }	        	
				if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/knowledgebase/')>0)
	            {
	                if($(value).attr('href').indexOf('/zh-CN/')>0){
						kbTrackerName = "从知识库搜索答案";
					}
					if($(value).attr('href').indexOf('/ja-JP/')>0){
						kbTrackerName = "ナレッジベース検索";
					}
					$(value).attr('href','javascript:kbSamlResponse()');
					$(value).text(kbTrackerName);
	                return;
	            }
	        });

	        if(document.URL.indexOf("/support/overview.html")>0)
	        {
	            var leftLinks = $('#midarea').find('a');
	            $.each( leftLinks, function(index, value) {
	            	if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/support/application-support.html')>0)
	                {
	                    $(value).attr('href','javascript:openSamlResponse()');
	    				$(value).text(requestTrackerName);
	                    return;
	                }
	            });
	        }else if(document.URL.indexOf("aboutus/contact-us.html")>0)
	        {
	            var leftLinks = $('#midarea').find('a');
	            $.each( leftLinks, function(index, value) {
	            	if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/tech_support/router.mvp')>0)
	                {
	                    $(value).attr('href','javascript:openSamlResponse()');
	    				$(value).text(requestTrackerName);
	                    return;
	                }
	            });
	        }
        }
//        else
//        {
//            setSupportByEmailInModalLink();
//        }
//             
        if(typeof profileCallback =="function" && document.URL.indexOf("/mymaxim/profile_preference.html")>0)
        	profileCallback();
    }
    else{
    	var visitor;
        if (typeof Visitor!=='undefined') {
            visitor = Visitor.getInstance("8D7A9CD35437E9A30A4C98A7@AdobeOrg");
            visitor.setCustomerIDs({
                 "userid":{
                     "id":CQ_Analytics.PersonalInfoStoreMgr.getProperty("hashId"),
                     "authState":2
                 }
            });
        }
    	$('.myMaximSpan').html("My Maxim");

		$('.myMaximSpan').parent().attr("href","#").click(function(){
			  gigya.accounts.showScreenSet(aem.login.params);
		});
		
    	$('.myMaximLink').addClass('disablelink');
        $('#myMaximLinkImg').addClass('disablelink');
        $('.myMaximLogoutDiv').hide();
        $('.myMaximLoginDiv').show();
        $('#myMaximLogoutDl').hide();
        $('#myMaximLoginDl').show();
        $('#mymi-icon').attr("src","/etc/designs/maximintegrated/_images/icons/mymaxim_icon.png");
        $('#mymi-icon-mobile').attr("src","/etc/designs/maximintegrated/_images/icons/mymaxim_icon.png").hide();
        $('#bookmarkNoLoginDiv').show();
        $('#bookmarkDiv').hide();

    	if(typeof lpMTagConfig !== 'undefined')
        {
    		lpMTagConfig.vars.push(["session","MemberCenter_id","0"]);
    		lpMTagConfig.vars.push(["session","Priority",""]);
        }

        setSupportByEmailInModalLink();
    }
}
function setSupportByEmailInModalLink()
{        
    var leftLinks = $('#leftmenu').find('a');
    $.each( leftLinks, function(index, value) {
        if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('tech_support/router.mvp')>0)
        {
            $(value).attr('href','javascript:supportByEmailInModal()');
            return;
        }
    });
    var mobileLeftLinks = $('.navbar-nav').find('a');
    $.each( mobileLeftLinks, function(index, value) {
        if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('tech_support/router.mvp')>0)
        {
            $(value).attr('href','javascript:supportByEmailInModal()');
            return;
        }
    });
    if(document.URL.indexOf("/support/overview.html")>0)
    {
        var leftLinks = $('#midarea').find('a');
        $.each( leftLinks, function(index, value) {
            if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/support/application-support.html')>0)
            {
                $(value).attr('href','javascript:supportByEmailInModal()');
                return;
            }
        });
    }else if(document.URL.indexOf("aboutus/contact-us.html")>0)
    {
        var leftLinks = $('#midarea').find('a');
        $.each( leftLinks, function(index, value) {
            if(typeof $(value).attr('href')!='undefined' && $(value).attr('href').indexOf('/tech_support/router.mvp')>0)
            {
                $(value).attr('href','javascript:supportByEmailInModal()');
                return;
            }
        });
    }
}
$(document).ready(function(){
    if (CQ_Analytics.PersonalInfoStoreMgr) {
        refreshProfileSnippet();
    }

    $("#bookmarksLink").click(function() {
    	bookMarksModalDisplay();
    });
    
    $("#bookmarksLink_img").click(function() {
    	bookMarksModalDisplay();
    });
    $('[data-toggle="tooltip"]').tooltip(); 
});

function bookMarksModalDisplay() {
    if (login.status) {
        $('#bookmarkNoLoginDiv').hide();
        $('#bookmarkDiv').show();
        initBookmark();
    }
    else
    {
        $('#bookmarkNoLoginDiv').show();
        $('#bookmarkDiv').hide();
    }

    $('#bookmarksModal').modal('show');
}
function refreshProfileSnippet() {
		var userProfileLoaderLink = "";
		
        if(typeof personalInfoLink != "undefined" && typeof personalInfoLink.url != "undefined") {
			userProfileLoaderLink = personalInfoLink.url;
        } else {
			userProfileLoaderLink = $('.personal-info-link').length > 0 ? $('.personal-info-link').data("personal-info-link") : "";
		}
		
		if(userProfileLoaderLink != "") {
			CQ_Analytics.PersonalInfoStoreMgr.init(userProfileLoaderLink, navBarProfileCallback);
		}
}

    
function lastClickedDate() {
    var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
    var bulletindate = new Date();
    var mymaximdate = bulletindate.toISOString();
    var lang=getUrlLanguage();
    var formData = "authorizableId=" + encodeURIComponent(authorizableId) + "&" + encodeURIComponent("profile/lastClickedDate") + "=" + encodeURIComponent(mymaximdate)+ "&" + encodeURIComponent("profile/language") + "=" + lang;
    resetMyMaximBulletin();
    jQuery.ajax({
        url: "/bin/AccountModificationServlet",
        type: "POST",
        contentType: "application/x-www-form-urlencoded",
        data: formData,
        success: function(data) {
            initMyMaximBulletin();
        },
        error: function(jqXHR, exception) {
            if (jqXHR.status === 0) {
                //alert("Not connect. Verify Network.");
            } else if (jqXHR.status == 404) {
                alert("Requested page not found. [404]");
            } else if (jqXHR.status == 500) {
                alert("Internal Server Error [500].");
            } else if (exception === 'parsererror') {
                alert("Requested JSON parse failed.");
            } else if (exception === 'timeout') {
                alert("Time out error.");
            } else if (exception === 'abort') {
                alert("Ajax request aborted.");
            } else {
                alert("Uncaught Error: " + jqXHR.responseText);
            }
        }
    });
}

function updateMyMaxList(categoryIDs, ids, action) {
    var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
    var mymaxcategoryString = categoryIDs.join(",");
    var mymaxstring = ids.join(",");
    var lang=getUrlLanguage();
    var mymaxData = "authorizableId=" + encodeURIComponent(authorizableId) + "&" + encodeURIComponent("profile/mymaxCategoryOrder") + "=" + encodeURIComponent(mymaxcategoryString)+"&" + encodeURIComponent("profile/mymaxOrder") + "=" + encodeURIComponent(mymaxstring);
    saveCacheProperty("mymaxCategoryOrder",mymaxcategoryString);
    if(action>0)
    {
        var bulletindate = new Date();
    	var mymaximdate = bulletindate.toISOString();
		mymaxData+="&" + encodeURIComponent("profile/lastClickedDate") + "=" + encodeURIComponent(mymaximdate)+ "&" + encodeURIComponent("profile/language") + "=" + lang;
        resetMyMaximBulletin();
        initMyMaximBulletin();
    }
    jQuery.ajax({
        url: "/bin/AccountModificationServlet",
        type: "POST",
        contentType: "application/x-www-form-urlencoded",
        data: mymaxData,
        success: function(data) {
            if (data.status) {
                ;
            } else
                alert(data.error);
        },
        error: function(jqXHR, exception) {
            if (jqXHR.status === 0) {
                //alert("Not connect. Verify Network.");
            } else if (jqXHR.status == 404) {
                alert("Requested page not found. [404]");
            } else if (jqXHR.status == 500) {
                alert("Internal Server Error [500].");
            } else if (exception === 'parsererror') {
                alert("Requested JSON parse failed.");
            } else if (exception === 'timeout') {
                alert("Time out error.");
            } else if (exception === 'abort') {
                alert("Ajax request aborted.");
            } else {
                alert("Uncaught Error: " + jqXHR.responseText);
            }
        }
    });
}

$(function() {
    var newFormat = [];
    var action = 0;
    var counterValue = parseInt($('.bubble').html());

    $('#bulletinTab').click(function() {
        action++;
        $('#bulletinTab').css("border-bottom","5px solid #6e2585");
        $('#mymaximTab').css("border-bottom","none");
		
        if (window.digitalDataSearch.page !== undefined) {
        	var data =  window.digitalDataSearch.page;
        	data.myModal = "myBulletin_Modal";
        }		
    });
    $('#mymaximTab').click(function() {
    	if(hasNewBulletin)
    	{
			lastClickedDate();
			hasNewBulletin=false;
		}
		
        $('#bulletinTab').css("border-bottom","none");
        $('#mymaximTab').css("border-bottom","5px solid #6e2585");
		
        if (window.digitalDataSearch.page !== undefined) {
        	var data = window.digitalDataSearch.page;
        	data.myModal = "myMaxim_Modal";
        }		
    });

    $('#myMaxDialog').on('hidden.bs.modal', function() {
        var categoryIds = [];
        $('.categroyHeadline').each(function() {
            var categoryId = $(this).text();
            categoryIds.push(categoryId);

        });
        var newMymaxsortCategoryOrder = categoryIds.join(",");
        var mymaxsortCategoryOrder = CQ_Analytics.PersonalInfoStoreMgr.getProperty("mymaxCategoryOrder");
		
        var ids = [];
        $('.mymax-id').each(function() {
            var currentimgid = $(this).attr("maxid");
            ids.push(currentimgid);

        });
        var mymaxstring = ids.join(",");
        var mymaxsortOrder = CQ_Analytics.PersonalInfoStoreMgr.getProperty("mymaxOrder");
        if((newMymaxsortCategoryOrder!=mymaxsortCategoryOrder && (newMymaxsortCategoryOrder != null && typeof newMymaxsortCategoryOrder !== 'undefined')) || (mymaxstring!=mymaxsortOrder)&& (mymaxstring != null && typeof mymaxstring !== 'undefined')){
            bootbox.confirm({
				message: CQ.I18n.get('saveConfirmation'),
				buttons: {
					cancel: {
						label: "No"
					},
					confirm: {
						label: "Yes"
					}
				},
				callback: function(result) {
					if (result) {
						CQ_Analytics.PersonalInfoStoreMgr.setProperty("mymaxCategoryOrder", newMymaxsortCategoryOrder);
						CQ_Analytics.PersonalInfoStoreMgr.setProperty("mymaxOrder", mymaxstring);
						updateMyMaxList(categoryIds, ids, action);
					}
				}
			});
        }
    });

    //sortMyMaxim();

    function removeAnimation() {
        setTimeout(function() {
            $('.bubble').removeClass('animating')
        }, 1000);
    }

});

function hover(id, imgsrc) {
    document.getElementById(id).src = imgsrc;
}

function unhover(id, imgsrc) {
    document.getElementById(id).src = imgsrc;
}
function showLoadingIndicator(imgId)
{
	$('#'+imgId).show();
    return false;
}

function netTermsStatusUpdate() {
    var netTermsStatus = CQ_Analytics.ProfileDataMgr.getProperty("NetTermsStatus");
    var currentUser = CQ_Analytics.ProfileDataMgr.getProperty("authorizableId");
    var checkedStatus = CQ_Analytics.ProfileDataMgr.getProperty("NetTermsStatusChecked");
    //alert(checkedStatus);
    if(netTermsStatus == "PENDING" && checkedStatus == "") {
        var url = "/bin/NetTermsServlet?authorizableId="+currentUser;
        CQ_Analytics.ProfileDataMgr.setProperty("NetTermsStatusChecked", true);
        $.ajax({
            type: "PUT",
            url:url,
            success: function(result){
                if(result.status == true) {
                    CQ_Analytics.ProfileDataMgr.setProperty("NetTermsStatus", result.termsStatus);

                }
            },
            /* error: function(jqXHR, exception) {
                CQ_Analytics.ProfileDataMgr.setProperty("NetTermsStatusChecked", true);
                if (jqXHR.status === 0) {
                    //alert("Not connect. Verify Network.");
                } else if (jqXHR.status == 404) {
                    alert("Requested page not found. [404]");
                } else if (jqXHR.status == 500) {
                    alert("Internal Server Error [500].");
                } else if (exception === 'parsererror') {
                    alert("Requested JSON parse failed.");
                } else if (exception === 'timeout') {
                    alert("Time out error.");
                } else if (exception === 'abort') {
                    alert("Ajax request aborted.");
                } else {
                    alert("Uncaught Error: " + jqXHR.responseText);
                }
             }*/
        });
    }
}

/*
	Standards Compliant Rollover Script
	Author : Lauren Kelly
	http://morphosinc.com/maxim/product.php
*/
$(document).ready(function(){
	$('#relResources .right-module').each(function(){
		var srcon = $(this).find('.imgover').attr('src');
        if(srcon){
            var ftypeon = srcon.substring(srcon.lastIndexOf('.'), srcon.length);
            var hsrcon = srcon.replace(ftypeon, '_o'+ftypeon);
            if($(this).find('.imgoveron').length == 0){
            $(this).find('.right-module-img').append('<img class="imgoveron" src="'+hsrcon+'" style="display:none; border:none;" />');
            $(this).hover(function(){
                $(this).find('.imgover').attr('style', 'display:none;');
                $(this).find('.imgoveron').attr('style', 'display:block; border:none;');
            },function(){
                $(this).find('.imgover').attr('style', 'display:block; border:none;');
                $(this).find('.imgoveron').attr('style', 'display:none;');
            });
            }
        }
	});
});
$(document).ready(
    function() {
		/* Search Text Field Dropdown Functions */
        $("#events-center-text-search").click(function() {
            $("#events-center-results").slideDown();
			$("#events-center-text-clear").show();
			$("#events-center-text-search").hide();
        });
		$("#events-center-text-search").click(function(){
			$("#events-center-show-img").toggleClass("rotate1 rotate2");
		});
		/* Results Link Dropdown Functions */
        $("#events-center-show").click(function() {
            $("#events-center-results").slideToggle();
        });
        if ($("html").hasClass("lt-ie10")) {
        	var events_deg=0;
            $("#events-center-text-search").click(function(){
            	events_deg+=180;
    			$("#events-center-show-img").rotate(events_deg);
    		});
    		$("#events-center-show").click(function(){
    			events_deg+=180;
    			$("#events-center-show-img").rotate(events_deg);
    		});
        }
        else{
            $("#events-center-text-search").click(function(){
    			$("#events-center-show-img").toggleClass("rotate1 rotate2");
    		});
    		$("#events-center-show").click(function(){
    			$("#events-center-show-img").toggleClass("rotate1 rotate2");
    		});
        }
        
        // New click rotation function to enable more than one on the same page with the same class ie. .extendablebutton
        $(document).on("click",".extendablebutton", function(){
            $(this).find("#events-center-show-img").toggleClass("rotate1 rotate2");
		});
        

});
/* Clear Search Functions */
var objDropDownMenu1 = document.getElementsByName("eventType")[0];
var objDropDownMenu2 = document.getElementsByName("regionFilter")[0];

function clearTextBox (form) {
    document.getElementById('textfield').value = '';
    alert("Clear Text Box");
	$("#events-center-text-clear").hide();
	$("#events-center-text-search").show();
}

function clearEventTypeFilter (form) {
    objDropDownMenu1.options[0].selected = true;
    alert("Clear Event Type Filter");
	$("#eventTypeAction").hide();
}

function clearRegionFilter (form) {
    objDropDownMenu2.options[0].selected = true;
    alert("Clear Region Filter");
	$("#regionAction").hide();
}



/* Product Family Function */
function eventTypeChanged(famValue) {
  alert("Event Type Filter: " + famValue);
	$("#eventTypeAction").show();
    $("#events-center-results").slideDown();
}

/* End Application Function */
function regionFilterChanged(appValue) {
  alert("Region Filter: " + appValue);
	$("#regionAction").show();
	$("#events-center-results").slideDown();	
}

