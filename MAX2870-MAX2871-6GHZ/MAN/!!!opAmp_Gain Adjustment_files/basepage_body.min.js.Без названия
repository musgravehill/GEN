          e.stopPropagation(),
            a.removeClass("active"),
            s.text($(this).text()).removeClass("active"),
            t.val($(this).attr("rel")),
            o.hide(),
            t[0].onchange && t[0].onchange();
        }),
        $(document).click(function() {
          a.removeClass("active"), o.hide();
        }),
        n
          ? (o.hover(
              function() {
                a
                  .addClass("active")
                  .closest(".nav-item")
                  .addClass("active")
                  .siblings(".overlay")
                  .addClass("active"),
                  o.show();
              },
              function() {
                ((i || $(".select-styled:hover").length) && !i) ||
                  (a.removeClass("active"), o.hide());
              }
            ),
            $(".overlay").mouseenter(function() {
              a
                .closest(".nav-item")
                .removeClass("active")
                .siblings(".overlay")
                .removeClass("active"),
                o.hide();
            }),
            o.mouseleave(function() {
              ((i || $(".select-options:hover").length) && !i) ||
                (a.removeClass("active"), o.hide());
            }))
          : o.mouseleave(function() {
              a.removeClass("active"), o.hide();
            });
    });
  }),
