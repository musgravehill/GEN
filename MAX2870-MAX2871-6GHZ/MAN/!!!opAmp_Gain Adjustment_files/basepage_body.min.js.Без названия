if((CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) == "true" || CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) === true) && (CQ_Analytics.PersonalInfoStoreMgr.getProperty("islegalagreedinprofile")!= "" && CQ_Analytics.PersonalInfoStoreMgr.getProperty("islegalagreedinprofile") != "true")) {
