function checkTLEnabled() {
	return "true"==document.getElementsByTagName("head")[0].getAttribute("mx_enablestl");
}
var customUrl = window.location.href,
	urlCleanChar = "-",
	cleanElqTaxonomy = [];
	
if ((typeof elq_taxonomy !== "undefined") && elq_taxonomy.join("").length){
	for (var i=0;i<elq_taxonomy.length;i++){
		cleanElqTaxonomy.push(elq_taxonomy[i].replace(/[\s]/g, urlCleanChar).replace(/[\&\#\?\/\\]/g, ""));
	}
	customUrl = "/" + cleanElqTaxonomy.join("/");
}

var _elq_initd = typeof(_elq_initd) == "undefined"? false : _elq_initd;
var _elqQ = _elqQ || [];

if (_elqQ.length == 0) {
    _elqQ.push(['elqSetSiteId', '248385305']);
    _elqQ.push(["elqTrackPageView"]);
    _elqQ.push(["elqUseFirstPartyCookie", window.location.host.replace("www.","")]);
}

function elq_auto_load()
{
    if (window.addEventListener) window.addEventListener('DOMContentLoaded', elq_boot, false);
    else if (window.attachEvent) window.attachEvent('onload', elq_boot);
}

function elq_boot()
{
    if (_elq_initd) { return (false); }
    elq_init();
    return (_elq_initd = true);
}

function elq_async_load() { return (elq_init()); } // old name
function elq_init()
{
   
    var s = document.createElement('script'); s.type = 'text/javascript'; s.async = true;
            s.src = '//img.en25.com/i/elqCfg.min.js';
            var x = document.getElementsByTagName('script')[0]; x.parentNode.insertBefore(s, x);
}

(function($){
	// tracking clicks on tabs links loading content to tabs asynchronously
	var _pageUrl = window.location.protocol + "//" + window.location.hostname + window.location.pathname;
	if (_pageUrl.substr(_pageUrl.length-1) === "/"){ _pageUrl = _pageUrl.substr(0, _pageUrl.length-1); }
	$("ul.nav-tabs li a").click(function(){
		if (($(this).attr("href").substr(0,1) === "#") && (typeof _elq !== "undefined") && (typeof _elq.trackEvent === "function")){
			var _label = $(this).text();
			if (typeof _label.trim === "function"){
				_label = _label.trim();
			}
			_label = _label.replace(/\s/g,"_");
			_elq.trackEvent( _pageUrl + "/" + _label );
		}
	});
	//tracking clicks on links to PDFs
	$("a[href$='.pdf'],a[href$='.PDF']").click(function(){
		if ((typeof _elq !== "undefined") && (typeof _elq.trackEvent === "function")){
			_elq.trackOutboundLink(this);
			return false;
		}
	});
})(jQuery)

