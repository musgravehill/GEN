		$("#training-videos-container").animate({width: 'toggle'},'fast').promise().done(function(){
        if(!isIE){
            $("div[class^='rightbar-module-']").each(function () { 
                    if($(this).attr('id').indexOf("-container") != -1 && $(this).attr('id') != "training-videos-container"){
                      $(this).hide();
                    }
            });
        }
		});
	});
	$("#training-videos-close").off(clickHandler).on(clickHandler, function () {
		$("#training-videos-container").animate({width: 'toggle'});
	});
	
