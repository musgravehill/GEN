$(window).load(function() {

	if (window.location.href.indexOf('#mycart') != -1) {
		setTimeout(function() {
			mycart_widget();
		}, 400)
	}

	$("#addToCartP").on('shown.bs.modal', function(e) {
    if ($('#distributor-modal').children().length <= 0) {
        var part_no = document.getElementById('Pcode').value;
    	if(!part_no){
    			part_no = getParameterByName("fert");
    	}
        getDistrbutorInventoryData(part_no);
    }
});

})

function logout(callback) {
    gigya.accounts.logout();
    if (typeof callback == "undefined") {
		var callbackUrl = decodeURIComponent(window.location.href.replace(/&?cq_ck=(\d*)?/, ""));
		if (callbackUrl.includes("?#")){
			callbackUrl=callbackUrl.replace("?#","#");
		}		
		callback = encodeURIComponent(callbackUrl);
    }
    $.removeCookie("hasaccesstoassetshare", {
        path: '/'
    });
    if (store.enabled) {
        store.clear();
    }
    if (_g && _g.shared && _g.shared.ClientSidePersistence) {
        _g.shared.ClientSidePersistence.clearAllMaps();
    }
    var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
    if (CQ.WCM == undefined) {
        if (CQ_Analytics && CQ_Analytics.CCM) {
            CQ_Analytics.ProfileDataMgr.clear();
            CQ_Analytics.CCM.reset();
        }
        CQ.shared.HTTP.clearCookie("CommercePersistence", "/");
        var url = "\/bin\/LogoutServlet?language=en\x26cugEnabled=false" + "&authorizableId=" + authorizableId;
        if (typeof callback != "undefined" && callback != "") {
            if (url.includes("storefront")) {
                url += "&callback=/en.html";
            } else {
                url += "&callback=" + callback;
            }

        }
        if (typeof _satellite != "undefined") {
            _satellite.track("logout success");
        }
        CQ.shared.Util.load(url);
        if (window.sessionStorage) {
            var tokenVal = (sessionStorage.getItem("tokenValue")) ? sessionStorage.getItem("tokenValue") : null;
            if (tokenVal !== null) {
                sessionStorage.removeItem("tokenValue");
            }
        }

    } else {
        if (CQ_Analytics && CQ_Analytics.CCM) {
            CQ_Analytics.ProfileDataMgr.loadProfile("anonymous");
            CQ.shared.Util.reload();
        }
    }

}

var cartModalContent = $("#myCartBody").html();
				
function mycart_widget(param, cb) {
    var currentUser = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
    var userType = encodeURI(CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType"));
    var cartJson = {};
    var data = "input=getCart";
    $.ajax({
        url: "/bin/mycart",
        type: 'POST',
        data: data,
        success: function(data) {
            if (data == "genericerrorText") {
                bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
            } else {
                try {
                    if (!data) {
                        cartJson = {};
                    } else {
                        cartJson = jQuery.parseJSON(data);
                    }
                } catch (e) {
                    console.log(e);
                }
                var isInternalCart = false;

                if (cartJson.hasOwnProperty("carts")) {
                    var sampleFlag = true;
                    var cartFlag = true;
                    if (cartJson.carts.length < 3) {
                        for (var i = 0; i < cartJson.carts.length; i++) {
                            if (cartJson.carts[i].cartType == "Cart") {
                                cartFlag = false;
                            } else if (cartJson.carts[i].cartType == "SampleCart") {
                                sampleFlag = false;

                                if (cartJson.carts[i].internalCart == true)
                                    isInternalCart = true;
                            }
                        }
                        if (cartFlag == true) {
                            cartJson.carts.push({
                                "cartType": "Cart"
                            });
                        }
                        if (sampleFlag == true) {
                            cartJson.carts.push({
                                "cartType": "SampleCart"
                            }); //To handle sample cart messages since it gets returned as array.
                            //alert(JSON.stringify(cartJson.carts));
                        }
                    }
                    digitalCartFunction(cartJson);
                    if (cb) {
                        cb(true);
                    }
                } else {
                    cartJson = {
                        "carts": [{
                            "cartType": "Cart"
                        }, {
                            "cartType": "SampleCart"
                        }]
                    };
                }

				
				if(typeof cartModalContent != "undefined") {
					var CartTemplate = Handlebars.compile(cartModalContent);
					$('#cartModalWidget').html(CartTemplate(cartJson));
				}
				$('#cartModalWidget').modal('show');
                if (param == "sample") {
                    $('#myTab a[href="#myCartSampleWidget"]').tab('show');
                } else {
                    $('#myTab a[href="#myCartOrderWidget"]').tab('show');
                }

                if (userType.toUpperCase() == "EMPLOYEE") {
                    $("#saveForLaterModal").hide();
                    $("#viewAllSavedLink").hide();
                    $('#myBuyTab').hide();
                    $('#mySampleTab a').click();
                    if (isInternalCart) {
                        $('#mySampleTab a').html("INTERNAL ORDER");
                        $('.sample-view-cart-button').attr('href', $('.sample-view-cart-button').attr('href') + "&userType=EMPLOYEE");
                    }
                }

            }
        }
    });

}

function pageBottom() {
    if (typeof _satellite != "undefined") {
        _satellite.pageBottom();
    }
}

function digitalCartFunction(jsonData) {
    var digitalCartData = {};
    var productInfo = {};
    var revenueItemArray = [];
    var sampleItemArray = [];
    var revenueItemJson = {};
    var sampleItemJson = {};
    for (var count in jsonData.carts) {
        if (null != jsonData.carts[count].entries) {
            for (var i in jsonData.carts[count].entries) {
                var productEntry = jsonData.carts[count].entries[i];
                if (jsonData.carts[count].cartType == "Cart") {
                    revenueItemArray.push({
                        productInfo: {
                            productID: productEntry.product.code,
                            stockStatus: productEntry.product.stock.stockLevelStatus,
                            quantity: productEntry.quantity,
                        }
                    });
                } else {
                    sampleItemArray.push({
                        productInfo: {
                            productID: productEntry.product.code,
                            stockStatus: productEntry.product.stock.stockLevelStatus,
                            quantity: productEntry.quantity,
                        }
                    });
                }
            }
        }
    }
    revenueItemJson = {
        item: revenueItemArray
    }

    sampleItemJson = {
        item: sampleItemArray
    }

    var quoteId = "";
    if (location.search.split("=")[0] == "?quoteId") {
        quoteId = location.search.split("=")[1]
    }
    var cartCategoryRevenue = "";
    var cartCategorySample = "";
    var cartCategoryQuick = "";
    var cartIdValue = "";
    for (var count in jsonData.carts) {
        if (count == 0 && jsonData.carts[count].cartType != null) {
            cartCategoryRevenue = jsonData.carts[count].cartType;
        } else if (count == 1 && jsonData.carts[count].cartType != null) {
            cartCategorySample = jsonData.carts[count].cartType;
        } else {
            cartCategory = "Quick Order";
        }
        if (count == 0 && jsonData.carts[count].code != null) {
            cartIdValue = jsonData.carts[count].code;
        }
    }
    window.digitalCartData = {
        stepCd: window.stepCd,
        cartId: cartIdValue,
        cart: revenueItemJson,
        sampleCart: sampleItemJson,
        revenueCartType: cartCategoryRevenue,
        sampleCartType: cartCategorySample,
        quickCartType: cartCategoryQuick,
        quoteID: quoteId,
    }
    if (null != quoteId && "" != quoteId && typeof _satellite != "undefined") {
        _satellite.track("apply-quote");
    }

}

function removeSampleCart(part_no, entry_no) {
    $.ajax({
        type: "post",
        url: '/bin/cartOperations',
        data: "input=deletecart&entry=" + entry_no + "&cartType=SampleCart&CartType=sampleCart",
        success: function(msg) {
            //alert("Part number "+partno+" Deleted Successfully");
            localStorage.removeItem(part_no);
            var url = window.location.href;
            if (url.indexOf("mycart_page") > -1 || url.indexOf("checkout_page") > -1) {
                location.reload();
            } else {
                mycart_widget("sample", function(status) {
                    if (status && typeof _satellite != "undefined") _satellite.track("cart: add");
                });
            }
            //document.forms["delete_cart"].submit();
        }
    });
}

function removeCart(partno, entry_no, message) {
    $.ajax({
        type: "post",
        url: '/bin/cartOperations',
        data: "input=deletecart&entry=" + entry_no,
        success: function(msg) {
            var url = window.location.href;
            if (msg.match("^Quote compliance not")) {
                 bootbox.alert(msg);
                 return false;
            } else if (url.indexOf("mycart_page") > -1 || url.indexOf("checkout_page") > -1) {
                location.reload();
            } else {
                mycart_widget("buy");
                if (typeof _satellite != "undefined") {
                    _satellite.track('cart: remove');
                }
            }
        }
    }); 
}


$(document).ready(function() {

    $('.mycart-widget').click(mycart_widget);

    $(function() {
        if ($('#breadcrumb2').attr('class') === "checkout-breadcrumb checkout-active pointer") {
            window.stepCd = "shipping_payment";
        }
    });

    function tabchange(a_i) {
        $('#myCartTab a[href="#' + a_i + '"]').tab('show');
    }

    // Saved for later code starts.
    $(document).on("click", ".saveForLaterModal", function() {
        var userId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
        var partNo = $(this).data("item-id");
        var entryNo = $(this).data("entry-id");
        if (userId == 'anonymous') {
            digitalErrorFunction(CQ.I18n.getMessage("logintoaddinsavedforlaterText"));
            bootbox.alert(CQ.I18n.getMessage("logintoaddinsavedforlaterText"));
        } else {
            $.ajax({
                url: "/bin/SavedForLaterServlet?authorizableId=" + userId + "&partNo=" + encodeURIComponent(partNo),
                type: "POST",
                dataType: "json",
                success: function(data) {
                    removeCart(partNo, entryNo, 'Save');
                },
                error: function(data, status) {
                    digitalErrorFunction('CQ.I18n.getMessage("genericerrorText")');
                }
            });
        }
    });
});

var digitalAssetPath = $('.digital-asset-path').length > 0 ? $('.digital-asset-path').data('digital-asset-path') : null;

if (digitalAssetPath != null) {
    window.assetspathcug = digitalAssetPath;
}
if ($.cookie('assetsharecomploc') == undefined) {
    $.cookie('assetsharecomploc', window.assetspathcug);
}

var isEloquaDisabled = $('.eloqua-disabled').length > 0 ? $('.eloqua-disabled').data('eloqua-disabled') : null;

if (typeof isEloquaDisabled == "undefined" || isEloquaDisabled == null || isEloquaDisabled != 'true') {
    elq_auto_load();
}

if (CQ.WCM == undefined) {
    addLinkerEvents();
}



function getPublicUrlLanguage() {
    var lang = "en_US";
    var urllanguage = getUrlLanguage();
    if (urllanguage === 'en') {
        lang = "en";
    } else if (urllanguage === 'ja' || urllanguage === 'jp') {
        lang = "jp";
        if (!isPublish)
            lang = "ja";
    } else if (urllanguage === 'cn' || urllanguage === 'zh') {
        lang = "cn";
        if (!isPublish)
            lang = "zh";
    }
    return lang;
}

var clients = ['mouser', 'digiKey', 'farnell'];
$(document).ready(function() {
    $(function() {
        $("#leadTimeToolTip").tooltip();
    });

    Handlebars.registerHelper('atLeast', function(v1, v2, v3, options) {
        if (typeof v1 != "undefined" || typeof v2 != "undefined" || typeof v3 != "undefined") {
            return options.fn(this);
        } else {
            return options.inverse(this);
        }
    });
    Handlebars.registerHelper('OrderCompilanceText', function(v1, options) {
        if (v1 == 0 && (v1 != "" && v1 != 0)) {
            return new Handlebars.SafeString(CQ.I18n.getMessage("orderanyqtyText"));
        }
        if (v1 == 100 && (v1 != "" && v1 != 0)) {
            return new Handlebars.SafeString(
                CQ.I18n.getMessage("orderexactqtyText")
            );
        }
        return new Handlebars.SafeString(
            "order at least " + v1 + "% of the quantity quoted in your first order, and any quantity for subsequent orders."
        );

    });

    $(document).on("click", ".requestQuoteFromBuyCart", function() {
        $('.bootbox-close-button').trigger('click');
        var part = $(this).data("part-no");
        requestAQuote(part);
    });

    $(document).on("click", ".stockAvaliability1", function() {
        $('#quantityLeadDiv').toggle(500);
    });

    $(document).on("keypress", "#product-quantity", function(e) {
        //if the letter is not digit then display error and don't type anything
        if (e.which != 8 && e.which != 0 && (e.which < 48 || e.which > 57)) {
            //display error message
            if (e.which != 13) {
                digitalErrorFunction("Digits Only");
                $("#errmsg").html("Digits Only").show().fadeOut("slow");
            }
            return false;
        }
    });
});


var maxInventory = "";
var minQty = 1;
var mult = 0;
var maxInventory = "";

function requestAQuote(partNo) {

    $("#addToCartP").modal('hide');
    var part_no;
    if (typeof partNo != "undefined" && partNo != "") {
        part_no = partNo;
    } else {

        part_no = $('#Pcode').val();
    }
    $('#partNo1').val(part_no);
    if (CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) || CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) == "true") {
        var userType = encodeURI(CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType"));
        if (userType == "DISTRIBUTOR") {
            openModelNSamlResponse();
            return false;
        } else {
            populateQuoteScreenData();
            $("#requestQuote").modal('show');
            $("#form #partNo").val(part_no);
            searchProductCatalog(part_no, "partList1");
        }
    } else {
        gigya.accounts.showScreenSet(aem.login.params);
        $.cookie("redirect", "requestAQuote");
        $.cookie("part_no", part_no);
    }
}

function buy_cart(part_no, buy, params, sampQty) {
        var stdPricingFlag = 0;
        var userType = encodeURI(CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType"));
        if (userType == "") {
            userType = "anonymous";
        }
        if (params == 'Quote') {
            $("#addToCartP").attr('data-quote-id', sampQty.quoteId);
            $("#addToCartP").attr('data-quote-item-number', sampQty.quoteItemNumber);

        }
        var b2bUnitActive = CQ_Analytics.ProfileDataMgr.getProperty("b2bUnitActive");
        if (b2bUnitActive == "false") {
            digitalErrorFunction(CQ.I18n.getMessage("accountinactiveText"));
			 bootbox.alert(CQ.I18n.getMessage("accountinactiveText"));
            
        } else {
            if (buy == "part") {
                var result;
                var small_template = Handlebars.compile($("#buyModalBody").html());

                var servletName = "/bin/SFComponent";
                var originUrl = window.location.origin;
                var domainName;
                if(originUrl.includes(".maximintegrated.com")){
                	domainName = originUrl.match(/.maximintegrated.com/).toString();
                }
                else if(originUrl.includes(".stg-maximintegrated.com")){
                	domainName = originUrl.match(/.stg-maximintegrated.com/).toString();
                }
                document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
                var url = servletName + "?input=single&query=" + encodeURIComponent(part_no);
                $.ajax({
                    url: url,
                    dataType: "json",
                    async: false,
                    type: 'POST',
                    xhrFields: {
                        withCredentials: true
                    },
                    success: function (data) {
                        result = data;
                        if (result == "genericerrorText") {
                            document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
                            digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                            bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                        } else if (result != "") {
                            if (result.errors != undefined && result.errors[0].message) {
                                document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
                                digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                                bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                            } else {
                                document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
                                var sampleJson = result;
                                sampleJson['userType'] = userType;
                                console.log(sampleJson);
                                var vol = sampleJson.volumePrices;
                                var noPricingMsg = 0;
                                minQty = sampleJson.min;
                                mult = sampleJson.mult;
                                if (vol != undefined && userType != "DISTRIBUTOR") {
                                    for (var i = 0; i < vol.length; i++) {
                                        var j = i + 1;
                                        var vol1 = "";
                                        if (j <= vol.length) {
                                            if (vol[i].minQuantity < 1000) {
                                                stdPricingFlag = 1;
                                                if (j == vol.length) {
                                                    vol1 = vol[i].minQuantity + "+";
                                                } else {
                                                    vol1 = vol[i].minQuantity + "-" + vol[i].maxQuantity;
                                                }
                                            } else if (vol[i].minQuantity == 1000) {
                                                vol1 = "1000+";
                                                stdPricingFlag = 1;
                                            } else {
                                                if (i == 0 && vol[i].minQuantity > 1000) {
                                                    //vol1 = "No standard pricing available";
                                                    noPricingMsg++;
                                                    delete sampleJson.volumePrices[i].formattedValue;
                                                } else {
                                                    delete sampleJson.volumePrices[i];
                                                    continue;
                                                }
                                            }
                                        } else if (j == vol.length && j == 1) {
                                            vol1 = vol[i].minQuantity + "-" + 24;
                                            stdPricingFlag = 1;
                                        }

                                        sampleJson.volumePrices[i].minQuantity = vol1;
                                    }
                                }
                                maxInventory = sampleJson.stock.atpInv;
                                leadTime = sampleJson.leadTime;
                                if (maxInventory == "" || maxInventory == undefined) {
                                    maxInventory = 0;
                                }

                                $('#addToCart').html(small_template(sampleJson));
                                if (sampleJson.cf == "true" && sampleJson.carrierType != "Tape") {
                                    $("#priceBreak").hide();
                                    $("#requestQuoteDiv").removeClass("hidden");
                                    //$("#quantity5000").show();

                                    stdPricingFlag = 0;
                                }
                                if (noPricingMsg > 0 || vol == undefined) {
                                    $("#priceBreak").html(CQ.I18n.getMessage("noStandardPricingAvailable"));
                                    $('#orderingquotetext').addClass("hidden");
                                    $("#orderingquotenoprice").removeClass("hidden");
                                    //$('#useExistingQuote').show(); 
                                    $('#requestQuoteDiv').removeClass("hidden");
                                    $("#priceBreak").removeClass("right-border");
                                    //stdPricingFlag = 1;
                                }

                                $('#buyParams').text(params);
                                //document.getElementById('buyParams').innerHTML = params;

                                if (userType == "DISTRIBUTOR" || userType == "EMPLOYEE") {
                                    $("#quantity5000").hide();
                                    $(".requestQuoteFromBuyCart").hide();
                                    if (userType == "DISTRIBUTOR") {
                                        $("#moqRow").show();
                                    }

                                }

                                if (stdPricingFlag == 0 && params != 'Quote') {
                                    if (userType != "DISTRIBUTOR") {
                                        //$("#quantity5000").hide();
                                        $("#buyModalRequestQuote ").removeClass("pull-right");
                                        $("#buyQuantityDiv").hide();
                                    } else {
                                        $("#orderFromDisti").hide();
                                        if (sampleJson.distiBookCost == undefined) {
                                            $("#modelNLink").html("<span class='glyphicon glyphicon-warning-sign' aria-hidden='true'></span>&nbsp; <font color='red'>"+CQ.I18n.getMessage("distiBookCostNotAvblMsg")+ CQ.I18n.getMessage("pleaseRequestQuote")+" </font> <a href='javascript:openModelNSamlResponse()' target=''>"+CQ.I18n.getMessage("modelN")+ "</a> ");

                                            $("#inventoryDetails").removeClass("col-sm-6");
                                            $("#inventoryDetails").addClass("col-sm-12");

                                        } else {
                                            var partNumber = encodeURIComponent(part_no);
                                            var htmlBulkOrder = "<div class='bulkOrder'><button onClick=\"redirectBulkOrder('" + partNumber + "')\" class='btn btn-primary btn-sm pull-right uppercase' type='button'>"+CQ.I18n.getMessage("bulkOrder")+"</button></div>";
                                            $("#buyQuantityDiv").html(htmlBulkOrder);
                                        }
                                    }
                                }
                                if (sampleJson.canQuote != true) {
                                    $("#requestQuoteDiv").hide();
                                }

                                if (sampleJson.leadTime == 0 && !(sampleJson.cf == "true" && sampleJson.carrierType == "Tape")) {
                                if($('#requestQuoteDiv').css('display') != "block")
                                {
                                    $('#forLeadtime').removeClass('hidden');      
                                    }
                                    //$('#quantity5000').show();
                                }

                                if (maxInventory == 0) {
                                    $("#stdleadTime").html('<b>'+CQ.I18n.getMessage("standardleadtimeText")+'</b>');
                                }
                                if (params == 'Quote') {
                                    $('#rowDiv').addClass("hidden");
                                    $("#quoteRow").removeClass("hidden");
                                    $("#nonQuote").addClass("hidden");
                                } else {
                                    if (leadTime != "" && leadTime != "undefined" && (parseInt(leadTime) > 0 && parseInt(leadTime) < 98) || parseInt(leadTime) == 0) {

                                    } else {
                                        if (userType != "DISTRIBUTOR") {
                                            $('#rowDiv div:not("#inventoryDetails,#requestQuoteDiv, #requestQuoteDiv2")').hide();
                                            if ($(".requestQuoteFromBuyCart").css('display') == 'none') {
                                                $("#contactSales").show();
                                            }
                                        }
                                    }
                                }

                                $('#addToCartP').modal('show');
                                if (userType == "EMPLOYEE") {
                                    $(document).find('#qtytobuyText').hide();
                                    $(document).find('#product-quantity').hide();
                                    $(document).find('#addToCartButton').hide();
                                    $(document).find('#employeeMsg').show();


                                }

                                if (sampleJson.cf == "true" && sampleJson.carrierType == "Tape" && maxInventory == 0) {
                                    $("#priceBreak").remove();
                                    $("#orderingquotetext").remove();
                                    $("#quantity5000").remove();
                                    $('#useExistingQuote').remove();
                                    $('#borderid').remove();
                                    $('#inventoryDetails').removeClass('col-sm-6');
                                    $('#inventoryDetails').addClass('col-sm-12');
                                    $('#inventoryDetails').css('padding-left', '55px');
                                    $('#inventoryDetails').css('padding-right', '55px');
                                    $("#sotPartswithnostock").html(CQ.I18n.getMessage("zeroInventoryErrorMsg"));

                                }

                            }
                        } else {
                            document.getElementById('buyParams').innerHTML = "Default";
                            digitalErrorFunction(CQ.I18n.getMessage("hybrisdowncontacttechnicalText"));
                            bootbox.alert(CQ.I18n.getMessage("hybrisdowncontacttechnicalText"));
                        }
                    }

                });

                var url = "/bin/checkQuoteAvailabilty?partNumber=" + encodeURIComponent(part_no)
                $.ajax({
                    url: url,
                    dataType: "json",
                    type: 'POST',
                    async: false,
                    xhrFields: {
                        withCredentials: true
                    },
                    success: function (data) {
                        result = data;
                        if (result != "") {
                            if (result.quoteList.quotes.quoteAvailable) {
                                var quoteAvailable = result.quoteList.quotes.quoteAvailable;
                                if (quoteAvailable == "true") {
                                    $("#ortext").removeClass("hidden");
                                    $('#useExistingQuote').show();
                                }

                            }

                        }

                    }
                });

                if (CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn") == "") {
                    $.cookie("redirect", "widget");
                    $.cookie("buy", buy);
                    $.cookie("part_no", encodeURIComponent(part_no));
                    $.cookie("sampQty", sampQty);
                    if (params == "ProductOrderTab") {
                        $.cookie("orderTab", "true");
                    }
                }


            } else if (buy == "sample" && (CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) || CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) == "true")) {
                if (userType == "DISTRIBUTOR" || userType == "EMPLOYEE") {
                    window.location.href = oldEcommerceUrl;
                    return false;
                }
                if (localStorage.getItem(part_no) == 1) {
                    mycart_widget("sample", function (status) {
                        if (status && typeof _satellite != "undefined") _satellite.track("cart: add");
                    });
                    return false;
                } else {
                    var data = "input=addtocart&part_no=" + encodeURIComponent(part_no) + "&CartType=sampleCart&sampleQty=" + sampQty + "&usertype=" + userType;

                    if (sampQty >= 1 || userType == "DISTRIBUTOR" || userType == "EMPLOYEE") {
                        $.ajax({
                            type: "post",
                            url: '/bin/validateSampleOrder',
                            data: data,
                            success: function (msg, textStatus, jqXHR) {
                                if (textStatus == "success") {
                                    if (typeof msg == 'string') {
                                        //responce will be string when part is added to cart, else it is json obj
                                        if (msg == "false") {
                                            digitalErrorFunction(CQ.I18n.getMessage("productexceedsmaxsampleText"));
                                            bootbox.alert(CQ.I18n.getMessage("productexceedsmaxsampleText"));
                                            return false;
                                        } else if (msg.match("^You") || msg.match("^Product")) {
                                            bootbox.confirm(CQ.I18n.getMessage("yoursessionexpiredText"), function (result) {
                                                var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
                                                CQ.shared.Util.load("\x2Fbin\x2FLogoutServlet\x3Flanguage\x3Den" + "&authorizableId=" + authorizableId);
                                                window.location = urlPrefix+"/mymaxim/login.html";
                                            });
                                        } else if (msg.match("^No orderable unit")) {
                                            digitalErrorFunction(msg);
                                            bootbox.alert(msg);
                                        } else {
                                            localStorage.setItem(part_no, "1");
                                            mycart_widget("sample", function (status) {
                                                if (status && typeof _satellite != "undefined") _satellite.track("cart: add");
                                            });
                                        }
                                    } else {
                                        var itemCount;var orderCount;
                                        if (msg.hasOwnProperty('output') && msg.output.hasOwnProperty('validateEmailDomain')) {
                                            itemCount = msg.output.validateEmailDomain.itemCount;
                                            orderCount = msg.output.validateEmailDomain.orderCount;
                                        }

                                        if (typeof orderCount != 'undefined' && parseInt(orderCount) >= 1) {
                                            bootbox.alert(CQ.I18n.getMessage("orderCountText"));
                                            return;
                                        } else if (typeof itemCount != 'undefined' && parseInt(itemCount) >= 3) {
                                            bootbox.alert(CQ.I18n.getMessage("itemCountOneText")+" <a href='../storefront/storefront.html'>"+CQ.I18n.getMessage("itemCountTwoText")+"</a>"+CQ.I18n.getMessage("itemCountThreeText"));
                                            return;

                                        } else if (msg.hasOwnProperty('error')) {
                                            if (msg.error == "sampleNotAlloewdForPart") {
                                                bootbox.alert("This part number is not allowed for sample order.");
                                            } else if (msg.error == "genericerrorText") {
                                                digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                                                 bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                                            }
                                        } else if (msg.hasOwnProperty('output') && msg.output.hasOwnProperty('validateEmailDomain')) {
                                            var sampleMessage = msg.output.validateEmailDomain.message;
                                            if (sampleMessage == 'SAMPLEEMAILDENIAL') {
                                                $('#sampleemaildenialerrordialog').modal('show');
                                            } else {
                                                $('#domainerrordialog').modal('show');
                                            }
                                        } else if (msg.hasOwnProperty('status') && msg.status == false) {
                                            if (msg.message.match("^Your session expired")) {
                                                bootbox.confirm(CQ.I18n.getMessage("yoursessionexpiredText"), function (result) {
                                                    var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
                                                    CQ.shared.Util.load("\x2Fbin\x2FLogoutServlet\x3Flanguage\x3Den" + "&authorizableId=" + authorizableId);
                                                    window.location = urlPrefix+"/mymaxim/login.html";
                                                });

                                            } else {
                                                digitalErrorFunction(msg.message);
                                                bootbox.alert(msg.message);
                                            }
                                        }
                                    }
                                } else {
                                    digitalErrorFunction(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
                                    bootbox.alert(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
                                }
                            }
                        });
                    } else {
                        digitalErrorFunction(CQ.I18n.getMessage("notenoughqtyavlblesampleText") + sampQty);
                        bootbox.alert(CQ.I18n.getMessage("notenoughqtyavlblesampleText") + sampQty);
                    }
                }

            } else {
                $.cookie("redirect", "widget");
                $.cookie("buy", buy);
                $.cookie("part_no", encodeURIComponent(part_no));
                $.cookie("sampQty", sampQty);
                if (params == "ProductOrderTab") {
                    $.cookie("orderTab", "true");
                }
                gigya.accounts.showScreenSet(aem.login.params);
            }
        }
    }

function redirect_rochester(part_no) {
    var url = 'https://api.rocelec.com/parts/MAX/1?callback=callback&q=' + part_no + '&key=Z3kk1y*';

    $.ajax({
        type: 'GET',
        url: url,
        async: false,
        jsonpCallback: 'callback',
        contentType: "application/json",
        dataType: 'jsonp',
        success: function(json) {
            if (json.parts[0] != undefined) {
                if ('permalink' in json.parts[0]) {
                    window.open(json.parts[0].permalink, '_blank');
                } else {
                    digitalErrorFunction('<div id="rochester-modal" data-item-id="' + part_no + '">' + CQ.I18n.getMessage("rochesterMessage") + '</div>');
                    bootbox.alert('<div id="rochester-modal" data-item-id="' + part_no + '">' + CQ.I18n.getMessage("rochesterMessage") + '</div>');
                }
            } else {
                if (typeof _satellite != "undefined") {
                    _satellite.track("rochester-popup");
                }
                bootbox.alert('<div id="rochester-modal" data-item-id="' + part_no + '">' + CQ.I18n.getMessage("rochesterMessage") + '</div>');
            }

        },
        error: function(e) {
            console.log(e.message);
            digitalErrorFunction(e.message);
        }
    });


}

function getDistrbutorInventoryData(partNumber) {
    if (typeof partNumber != "undefined" && partNumber != "") {
        var distributors = {};
        var distributor_template = Handlebars.compile($("#distributorData").html());
        var data = "query=" + encodeURIComponent(partNumber);
        $.ajax({
            type: "post",
            url: '/bin/distributorInventoryComponent',
            async: true,
            data: data,
            success: function(data) {
                if (data.error == "genericerrorText") {
                    digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                    bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                } else if (data != null || data != "") {
                    $('#distributor-loading-image').hide();
                    if (data != "") {
                        var dataJSON = JSON.parse(data);
                        dataJSON['code'] = partNumber;
                        $('#distributor-modal').html(distributor_template(dataJSON));
                    }
                }
            }
        });

    }
}

function add_to_cart(part_no, buy, params, sampQty, quantity) {
        var userType = encodeURI(CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType"));
        if (userType == "") {
            userType = "anonymous";
        }
       
        if (params == 'Quote') {
            $("#addToCartP").attr('data-quote-id', sampQty.quoteId);
            $("#addToCartP").attr('data-quote-item-number', sampQty.quoteItemNumber);
        }
        if (buy == "part") {
            var result;
            var small_template = Handlebars.compile($("#buyModalBody").html());
            var originUrl = window.location.origin;
            var domainName;
            if(originUrl.includes(".maximintegrated.com")){
            	domainName = originUrl.match(/.maximintegrated.com/).toString();
            }
            else if(originUrl.includes(".stg-maximintegrated.com")){
            	domainName = originUrl.match(/.stg-maximintegrated.com/).toString();
            }
            document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
            var servletName = "/bin/SFComponent";
            var url = servletName + "?input=single&query=" + encodeURIComponent(part_no);
            $.ajax({
                url: url,
                dataType: "json",
                type: 'POST',
                xhrFields: {
                    withCredentials: true
                },
                success: function (data) {
                    result = data;
                    if (result == "genericerrorText") {
                        document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
                        digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                        bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                    } else if (result != "") {
                        if (result.errors != undefined && result.errors[0].message) {
                            document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
                            digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                            bootbox.alert(CQ.I18n.getMessaget("genericerrorText"));
                        } else {
                            document.cookie = "mySearchServlet=" + window.location + ";domain=" + domainName + ";path=/";
                            var sampleJson = result;
                            var vol = sampleJson.volumePrices;
                            sampleJson.userType = userType;
                            var noPricingMsg = 0;
                            minQty = sampleJson.min;
                            mult = sampleJson.mult;
                            if (vol != undefined && userType != "DISTRIBUTOR") {
                                for (var i = 0; i < vol.length; i++) {
                                    var j = i + 1;
                                    var vol1 = "";
                                    if (j <= vol.length) {
                                        if (vol[i].minQuantity < 1000) {
                                            stdPricingFlag = 1;
                                            if (j == vol.length) {
                                                vol1 = vol[i].minQuantity + "+";
                                            } else {
                                                vol1 = vol[i].minQuantity + "-" + vol[i].maxQuantity;
                                            }
                                        } else if (vol[i].minQuantity == 1000) {
                                            vol1 = "1000+";
                                            stdPricingFlag = 1;
                                        } else {
                                            if (i == 0 && vol[i].minQuantity > 1000) {
                                                //vol1 = "No standard pricing available";
                                                noPricingMsg++;
                                                delete sampleJson.volumePrices[i].formattedValue;
                                            } else {
                                                delete sampleJson.volumePrices[i];
                                                continue;
                                            }
                                        }
                                    } else if (j == vol.length && j == 1) {
                                        vol1 = vol[i].minQuantity + "-" + 24;
                                        stdPricingFlag = 1;
                                    }
                                    sampleJson.volumePrices[i].minQuantity = vol1;
                                }
                            }
                            maxInventory = sampleJson.stock.atpInv;
                            leadTime = sampleJson.leadTime;
                            if (maxInventory == "" || maxInventory == undefined) {
                                maxInventory = 0;
                            }

                            $('#addToCart').html(small_template(sampleJson));
                            if (userType != "DISTRIBUTOR") {
                                call(quantity);
                            }
                        }
                    } else {
                        document.getElementById('buyParams').innerHTML = "Default";
                        digitalErrorFunction(CQ.I18n.getMessage("hybrisdowncontacttechnicalText"));
                        bootbox.alert(CQ.I18n.getMessage("hybrisdowncontacttechnicalText"));
                    }
                }
            });
            

        } else if (buy == "sample") {
            if (userType == "DISTRIBUTOR" || userType == "EMPLOYEE") {
                window.location.href = oldEcommerceUrl;
                return false;
            }
            if (localStorage.getItem(part_no) == 1) {
                setTimeout(function() { mycart_widget("sample", function (status) {
                    if (status && typeof _satellite != "undefined") _satellite.track("cart: add");
                })},2000);
                return false;
            } else {
                var data = "input=addtocart&part_no=" + encodeURIComponent(part_no) + "&CartType=sampleCart&sampleQty=" + sampQty + "&usertype=" + userType;

                if (sampQty >= 1 || userType == "DISTRIBUTOR" || userType == "EMPLOYEE") {
                    $.ajax({
                        type: "post",
                        url: '/bin/validateSampleOrder',
                        data: data,
                        success: function (msg, textStatus, jqXHR) {
                            if (textStatus == "success") {
                                //responce will be string when part is added to cart, else it is json obj
                                if (typeof msg == 'string') {
                                    if (msg == "false") {
                                        digitalErrorFunction(CQ.I18n.getMessage("productexceedsmaxsampleText"));
                                        bootbox.alert(CQ.I18n.getMessage("productexceedsmaxsampleText"));
                                        return false;
                                    } else if (msg.match("^You") || msg.match("^Product")) {
                                        bootbox.confirm(CQ.I18n.getMessage("yoursessionexpiredText"), function (result) {
                                            var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
                                            CQ.shared.Util.load("\x2Fbin\x2FLogoutServlet\x3Flanguage\x3Den" + "&authorizableId=" + authorizableId);
                                            window.location = urlPrefix+"/mymaxim/login.html";
                                        });
                                    } else if (msg.match("^No orderable unit")) {
                                        digitalErrorFunction(msg);
                                        bootbox.alert(msg);
                                    } else {
                                        localStorage.setItem(part_no, "1");
                                        setTimeout(function() { mycart_widget("sample", function (status) {
                                            if (status && typeof _satellite != "undefined") _satellite.track("cart: add");
                                        })},2000);
                                    }
                                } else {
                                    var itemCount;var orderCount;
                                    if(msg.hasOwnProperty('output') &&  msg.output.hasOwnProperty('validateEmailDomain')) {
                                        itemCount = msg.output.validateEmailDomain.itemCount;
                                        orderCount = msg.output.validateEmailDomain.orderCount;
                                    }
                                    if (typeof orderCount != 'undefined' && parseInt(orderCount) >= 1) {
                                        bootbox.alert(CQ.I18n.getMessage("orderCountText"));
                                        return;

                                    } else if (typeof itemCount != 'undefined' && parseInt(itemCount) >= 3) {
                                        bootbox.alert(CQ.I18n.getMessage("itemCountOneText")+" <a href='../storefront/storefront.html'>"+CQ.I18n.getMessage("itemCountTwoText")+"</a>"+CQ.I18n.getMessage("itemCountThreeText"));
                                        return;

                                    } else if (msg.hasOwnProperty('error')) {
                                        if (msg.error == "sampleNotAlloewdForPart") {
                                            bootbox.alert("Part is not allowed for sample orders");
                                        } else if (msg.error == "genericerrorText") {
                                            digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                                            bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                                        }
                                    } else if (msg.hasOwnProperty('output') && msg.output.hasOwnProperty('validateEmailDomain')) {
                                        var sampleMessage = msg.output.validateEmailDomain.message;
                                        if (sampleMessage == 'SAMPLEEMAILDENIAL') {
                                            $('#sampleemaildenialerrordialog').modal('show');
                                        } else {
                                            $('#domainerrordialog').modal('show');
                                        }
                                    } else if (msg.hasOwnProperty('status') && msg.status == false) {
                                        if (msg.message.match("^Your session expired")) {
                                            bootbox.confirm(CQ.I18n.getMessage("yoursessionexpiredText"), function (result) {
                                                var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
                                                CQ.shared.Util.load("\x2Fbin\x2FLogoutServlet\x3Flanguage\x3Den" + "&authorizableId=" + authorizableId);
                                                window.location = urlPrefix+"/mymaxim/login.html";
                                            });
                                        } else {
                                            digitalErrorFunction(msg.message);
                                            bootbox.alert(msg.message);
                                        }
                                    }
                                }
                            } else {
                                digitalErrorFunction(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
                                bootbox.alert(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
                            }
                        }
                    });
                } else {
                    digitalErrorFunction(CQ.I18n.getMessage("notenoughqtyavlblesampleText") + sampQty);
                    bootbox.alert(CQ.I18n.getMessage("notenoughqtyavlblesampleText") + sampQty);
                }
            }
        }
    }
$(document).on("click", ".useThisBuyQuote", function() {
    var quantity = "";
    quantity = $('#product-quantityBuyQuote').val();
    quantity = quantity.trim();
    if (quantity.length == 0) {
        digitalErrorFunction(CQ.I18n.getMessage("enterqtyProceed"));
        bootbox.alert(CQ.I18n.getMessage("enterqtyProceed"));
        quantity.focus();
        return false;
    }
    var part_no = $('#Pcode').val();
    var quoteId = $(this).data("quote-id");
    var quoteItemId = $(this).data("quote-item-id");
    var urlPrefix = $('.url-prefix').length > 0 ? $('.url-prefix').data('url-prefix') : "";
    $.ajax({
        type: "post",
        url: '/bin/cartOperations',
        data: "input=addtocart&quoteId=" + quoteId + "&quoteItemNumber=" + quoteItemId + "&qty=" + quantity + "&part_no=" + encodeURIComponent(part_no),
        success: function(msg, textStatus, jqXHR) {


            if (textStatus == "success") {

                if (msg.match("^You") || msg.match("^Product")) {
                    if (msg.match("^Your session expired")) {
                        bootbox.confirm(CQ.I18n.getMessage("yoursessionexpiredText"), function(result) {
                            var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
                            CQ.shared.Util.load("\x2Fbin\x2FLogoutServlet\x3Flanguage\x3Den" + "&authorizableId=" + authorizableId);
                            window.location = urlPrefix + "/mymaxim/login.html";
                        });
                    } else {
                        digitalErrorFunction(msg);
                        bootbox.alert(msg);
                        return false;
                    }
                } else if (msg.match("^No Price found")) {
                    //bootbox.alert(msg);
                    digitalErrorFunction(CQ.I18n.getMessage("requestquotetoorder"));
                    bootbox.alert(CQ.I18n.getMessage("requestquotetoorder"));
                    return false;
                } else if (msg.match(CQ.I18n.getMessage("requestquotetoorder"))) {
                    digitalErrorFunction(msg);
                    bootbox.alert(msg);
                    return false;

                } else if (msg == "") {
                    digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                    bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                    return false;
                } else if (quoteId != "" && quoteId != undefined && !msg.match("statusCode=success")) {
                    digitalErrorFunction(msg);
                    bootbox.alert(msg);
                    return false
                } else {
                    $('#useQuoteModal').modal('hide');
                    mycart_widget();
                    if ($("#buyParams").html() == "SavedLater") {
                        //alert("Yes inside");
                        deleteSavedForLater(part_no);
                    }
                }
            } else {
                digitalErrorFunction(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
                bootbox.alert(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
            }
        }
    });
});

function call(quantity) {

    var qty = "",
        quoteId, quoteItemNumber = "";

    if (typeof quantity != "undefined" && quantity != "") {
        qty = quantity;
    } else {
        var value = $('#product-quantity1').val();
        if (value != "undefined" && value != "") {
            qty = value;
        } else {
            qty = document.getElementById('product-quantity').value;
        }
    }
    qty = qty.trim();

    var orderInMult = $('#orderInMult').val();
    var orderInMin = $('#orderInMin').val();
    var inventoryVal = $('#stockAvaliability').val() == "" ? "0" : $('#stockAvaliability').val();
    var stockAvaliability = $('#stockAvaliability').val() == "" ? "0" : $('#stockAvaliability').val();
    var urlPrefix = $('.url-prefix').length > 0 ? $('.url-prefix').data('url-prefix') : "";
    var cfValue = $('#cfValue').val();
    var carrierTypeValue = $('#carrierTypeValue').val();
    quoteId = $("#addToCartP").attr('data-quote-id');
    quoteItemNumber = $("#addToCartP").attr('data-quote-item-number');
    if (quoteId == undefined) {
        quoteId = "";
        quoteItemNumber = "";
    }
    if (quoteId != "" && quoteId != undefined) {

    } else {
        if (!(cfValue == 'true' && carrierTypeValue == 'Tape')) {
            if (parseInt(qty) > parseInt(inventoryVal)) {
                if (qty != "" && orderInMult != "" && orderInMin != "") {
                    if (parseInt(qty) < parseInt(orderInMin)) {
                        $('#product-quantity').val(orderInMin);
                        $('#quantity-label').show();
                        return false;
                    } else if (qty % orderInMult == 0) {
                        $('#quantity-label').hide();
                    } else {
                        var modvalue = parseInt(qty) % parseInt(orderInMult);
                        var q4 = parseInt(orderInMult) - parseInt(modvalue);
                        var nearestNum = parseInt(qty) + parseInt(q4);
                        //var nearestNum = (Math.round(qty / orderInMult)+1) * orderInMult;
                        $('#product-quantity').val(nearestNum);
                        $('#quantity-label').show();
                        return false;
                    }

                }

            } else if (parseInt(qty) <= parseInt(inventoryVal) && carrierTypeValue == 'Reel') {
                if (qty != "" && orderInMult != "" && orderInMin != "") {
                    if (parseInt(qty) < parseInt(orderInMin)) {
                        $('#product-quantity').val(orderInMin);
                        $('#quantity-label').show();
                        return false;
                    } else if (qty % orderInMult == 0) {
                        $('#quantity-label').hide();
                    } else {
                        var modvalue = parseInt(qty) % parseInt(orderInMult);
                        var q4 = parseInt(orderInMult) - parseInt(modvalue);
                        var nearestNum = parseInt(qty) + parseInt(q4);
                        //var nearestNum = (Math.round(qty / orderInMult)+1) * orderInMult;
                        $('#product-quantity').val(nearestNum);
                        $('#quantity-label').show();
                        return false;
                    }

                }
            }
        }

        // Bulk SOT Check
        if (cfValue == 'true' && carrierTypeValue == 'Tape') {
            if (parseInt(stockAvaliability) == 0) {
                digitalErrorFunction(CQ.I18n.getMessage("zeroInventoryErrorMsg"));
                bootbox.alert(CQ.I18n.getMessage("zeroInventoryErrorMsg"));
                return false;

            }

        }

    }



    var part_no = document.getElementById('Pcode').value;
    if (qty.length == 0) {
        digitalErrorFunction(CQ.I18n.getMessage("enterqtyProceed"));
        bootbox.alert(CQ.I18n.getMessage("enterqtyProceed"));
        qty.focus();
        return false;
    }
    if (qty % 1 != 0) {
        digitalErrorFunction(CQ.I18n.getMessage("positiveallowedText"));
        bootbox.alert(CQ.I18n.getMessage("positiveallowedText"));
        return false;
    }
    var userId = CQ_Analytics.ProfileDataMgr.getProperty("email");
    if (CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) || CQ_Analytics.PersonalInfoStoreMgr.getProperty("isLoggedIn", true) == "true") {
        var userType = encodeURI(CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType"));

        if (typeof quantity != "undefined") {
            $.removeCookie("redirect");
            $.removeCookie("part_no");
            $.removeCookie("sampQty");
            $.removeCookie("quantity");
            $.removeCookie("buy");
        }
        var data = "input=addtocart&quoteId=" + quoteId + "&quoteItemNumber=" + quoteItemNumber + "&qty=" + qty + "&part_no=" + encodeURIComponent(part_no);

        $.ajax({
            type: "post",
            url: '/bin/validateRevenueOrder',
            async: false,
            data: data,
            success: function (msg, textStatus, jqXHR) {
                    if (textStatus == "success") {
                        //responce will be string when part is added to cart, else it is json obj
                        if (typeof msg != 'string') {
                            if (msg.error == "genericerrorText") {
                                digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                                bootbox.alert(CQ.I18n.getMessage("genericerrorText"));
                            } else {
                                var isOrderAllowed = msg.output.validateEmailDomain.isOrderAllowed;
                                if (typeof isOrderAllowed != 'undefined' && isOrderAllowed != "true") {
                                    digitalErrorFunction(CQ.I18n.getMessage("genericerrorText"));
                                    $('#revenuedomainerrordialog').modal('show');
                                    return;
                                }
                            }
                        } else {
                            if (msg.match("^You") || msg.match("^Product")) {
                                if (msg.match("^Your session expired")) {
                                    bootbox.confirm(CQ.I18n.getMessage("yoursessionexpiredText"), function (result) {
                                        var authorizableId = CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId");
                                        CQ.shared.Util.load("\x2Fbin\x2FLogoutServlet\x3Flanguage\x3Den" + "&authorizableId=" + authorizableId);
                                        //window.location="<%=urlPrefix%>/storefront.html";
                                        window.location = urlPrefix+"/mymaxim/login.html";
                                    });
                                } else {
                                    digitalErrorFunction(msg);
                                    bootbox.alert(msg);
                                    return false;
                                }
                            } else if (msg.match("^No Price found")) {
                                //bootbox.alert(msg);
                                digitalErrorFunction(CQ.I18n.getMessage("requestquotetoorder"));
                                bootbox.alert(CQ.I18n.getMessage("requestquotetoorder"));
                                return false;
                            } else if (msg.match(CQ.I18n.getMessage("requestquotetoorder"))) {
                                digitalErrorFunction(msg);
                                bootbox.alert(msg);
                                return false;
                            } else if (msg.match("^zeroInventoryErrorMsg")) {
                                digitalErrorFunction(CQ.I18n.getMessage("zeroInventoryErrorMsg"));
                                bootbox.alert(CQ.I18n.getMessage("zeroInventoryErrorMsg"));
                            } else if (msg == "") {
                                digitalErrorFunction('System error occured. Contact customer support for assistance.');
                                bootbox.alert('System error occured. Contact customer support for assistance.');
                                return false;
                            } else if (quoteId != "" && quoteId != undefined && !msg.match("statusCode=success")) {
                                digitalErrorFunction(msg);
                                var msgMatch = msg.match("^Quote compliance not met for quote number (.[0-9]*) as compliance is (.[0-9]%)$");
								var msgMOQ=/^This part has a minimum order quantity \(MOQ\) that must be met in a single delivery. You must order a minimum of (.[0-9]*) units$/.test(msg);
                                if (msgMatch) {
                                    bootbox.alert(CQ.I18n.getMessage("quoteCompliancePart1") + msgMatch[1] + CQ.I18n.getMessage("quoteCompliancePart2") + msgMatch[2]);              
                                } 
								 else if(msgMOQ)
                                {
   									var num=msg.match(/(\d+)/g);
                                    bootbox.alert(CQ.I18n.getMessage("moqPart1") + num  + ' ' + CQ.I18n.getMessage("moqPart2"));				
                                }
								else {
                                    bootbox.alert(msg);
                                }
                                return false
                            } else if (msg.match('To order this part, you must order the entire inventory of')) {
                                bootbox.alert(msg);
                                return false;
                            } else if (msg.match('We are sorry! Unfortunately, there is not enough excess inventory to support this bulk SOT part request')) {
                                bootbox.alert(msg);
                                return false;
                            } else if (msg.match('A valid quote is required to purchase this part')) {
                                bootbox.alert(msg);
                                return false;
                            } else {
                                $('#addToCartP').modal('hide');
                                mycart_widget("", function (status) {
                                    if (status && typeof _satellite != "undefined") _satellite.track("cart: add");
                                    if (msg.match("entryNumber=1") && typeof _satellite != "undefined") {
                                        _satellite.track("cart: open");
                                    }
                                });

                                if (quoteId != null && quoteId != "" && quoteId != undefined && msg.match("statusCode=success")) {
                                    window.digitalQuoteData = {
                                        quoteID: quoteId,
                                        quoteQuantity: qty,
                                        partNo: encodeURIComponent(part_no)
                                    }
                                    if (typeof _satellite != "undefined") {
                                        _satellite.track("use-quote");
                                    }
                                }
                                if ($("#buyParams").html() == "SavedLater") {
                                    //alert("Yes inside");
                                    deleteSavedForLater(part_no);
                                }
                            }
                        }
                    } else {
                        digitalErrorFunction(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
                        bootbox.alert(CQ.I18n.getMessage("mostprobhybrisissueText") + JSON.stringify(msg));
                    }
                }
            });
        }else{
            $.cookie("quantity",qty);
            $("#addToCartP").modal("hide");
            gigya.accounts.showScreenSet(aem.login.params);
        }
    }

function redirectBulkOrder(partNumber, quantity) {
    var urlPrefix = $('.url-prefix').length > 0 ? $('.url-prefix').data('url-prefix') : "";
    var url = urlPrefix + "/storefront/bulk-order.html";
    if (typeof partNumber != "undefined" && partNumber != "") {
        url += "?partNumber=" + partNumber;
    }
    if (typeof quantity != "undefined" && quantity != "") {
        url += "&quantity=" + quantity;
    }
    window.location.href = url;
}

function redirectToModelN() {
    window.open("https://maxim.modeln.com/", '_blank', "width=400,height=150").focus();
}

$(document).on('click', '.quoteDetails', function() {
    $("#useQuoteModal .modal-body").html("");
    var index = "";
    var quoteNo = "";
    var part = $(this).data("part-no");
    if (part == "" || part == "undefined") {
        digitalErrorFunction(CQ.I18n.getMessage("pleaseEnterPartNumber"));
        bootbox.alert(CQ.I18n.getMessage("pleaseEnterPartNumber"));
        return false;
    }
    if (quoteNo != "" && quoteNo != "undefined") {
        getQuotesList(part, index, quoteNo);
    } else {
        getQuotesList(part, index, quoteNo);
        digitalErrorFunction(CQ.I18n.getMessage("pleaseEnterQuoteNumber"));
    }
});


function getQuotesList(part, index, quoteId) {
    var userType = CQ_Analytics.PersonalInfoStoreMgr.getProperty("userType");
    var oldPart = $("#partHidden" + index).val();
    //oldpart == part to handle unnecessary quotelists api calls for same part.
    if (part == "" || part == undefined || part.length < 5 || oldPart == part) {
        return false;
    }
    if (userType == "" || userType == undefined) {
        $("#modelBuyCartButton").trigger("click");
        gigya.accounts.showScreenSet(aem.login.params);
        return false;
    }
    var queryString = "quoteId=" + quoteId + "&pendingStatus=&summery=disti&quantity=&materialNumber=" + encodeURIComponent(part);
    var materialNumber = part;
    var activeFlag = 0;
    $.ajax({
        url: "/bin/QuoteMangementServlet",
        type: 'GET',
        data: queryString,
        success: function(data) {
            //alert(JSON.stringify(data));
			var useQuoteModalTemplate = $("#useQuoteModalBody").html();
            var useQuoteTemplate = Handlebars.compile(useQuoteModalTemplate);
            var useQuoteJson = {
                quotesList: [],
                code: materialNumber,
                index: index
            };
            if (data.status == false) {
                var useQuoteJson = {
                    quotesList: [],
                    code: materialNumber,
                    index: index,
                    errorMessage: data.error.errorList.errors.message
                };
                $("#modelBuyCartButton").trigger("click");
                $("#quoteLineFlag" + index).val("false");
                $('#useQuoteModal').html(useQuoteTemplate(useQuoteJson));
                $('#useQuoteModal').modal('show');
                digitalErrorFunction(data.error.errorList.errors.message);
                return false;
            } else {
                var quoteListJson = data.quoteList.quotes.quote;
            }
            if ($.type(data.quoteList.quotes.quote) != "array") {
                if ($.type(quoteListJson.quote_items) != "array") {
                    if (quoteListJson.quote_items.product.code == materialNumber) {
                        var quoteId = quoteListJson.ecom_qt_id_h;
                        var quantity = quoteListJson.quote_items.quantity;
                        //var price = findPriceByQuantity(quoteListJson.quote_items.product.volumePrices, quoteListJson.quote_items.quantity);
                        price = (quoteListJson.quote_items.requote_price * 1).toFixed(3);
                        if (userType == "DISTRIBUTOR") {
                            price = (quoteListJson.quote_items.adj_disti_cost * 1).toFixed(3);
                            quoteId = quoteListJson.modelNQuoteNumber;
                        }
                        var total = price * quantity;
                        total = total.toFixed(3);
                        if (quoteListJson.quote_items.quoteItemStatus == "ACTIVE" && quantity != "" && quantity != "undefined") {
                            activeFlag = 1;
                            useQuoteJson.quotesList.push({
                                "totalCost": total,
                                "quoteItemId": quoteListJson.quote_items.code,
                                "status": quoteListJson.quote_items.quoteItemStatus,
                                "quantity": quantity,
                                "entryNumber": quoteListJson.quote_items.item_num,
                                "quoteId": quoteId,
                                "price": price,
                                "orderCompilance": quoteListJson.quote_items.ordcompliance
                            });
                        }
                    } else {
                        //return useQuoteJson;
                        //break;
                    }
                } else {
                    for (var i = 0; i < quoteListJson.quote_items.length; i++) {
                        if (quoteListJson.quote_items[i].product.code == materialNumber) {
                            var quantity = quoteListJson.quote_items[i].quantity;
                            var quoteId = quoteListJson.ecom_qt_id_h;
                            //var price = findPriceByQuantity(quoteListJson.quote_items[i].product.volumePrices, quoteListJson.quote_items[i].quantity);
                            price = (quoteListJson.quote_items[i].requote_price * 1).toFixed(3);
                            if (userType == "DISTRIBUTOR") {
                                price = (quoteListJson.quote_items[i].adj_disti_cost * 1).toFixed(3);
                                quoteId = quoteListJson.modelNQuoteNumber;
                            }
                            var total = price * quantity;
                            total = total.toFixed(3);
                            if (quoteListJson.quote_items[i].quoteItemStatus == "ACTIVE" && quantity != "" && quantity != "undefined") {
                                activeFlag = 1;
                                useQuoteJson.quotesList.push({
                                    "totalCost": total,
                                    "quoteItemId": quoteListJson.quote_items[i].code,
                                    "status": quoteListJson.quote_items.quoteItemStatus,
                                    "quantity": quantity,
                                    "entryNumber": quoteListJson.quote_items[i].item_num,
                                    "quoteId": quoteId,
                                    "price": price,
                                    "orderCompilance": quoteListJson.quote_items[i].ordcompliance
                                });
                            }
                        } else {
                            continue;
                        }
                    }
                }
            } else {
                for (var j = 0; j < data.quoteList.quotes.quote.length; j++) {
                    var quoteListJson = data.quoteList.quotes.quote[j];
                    if ($.type(quoteListJson.quote_items) != "array") {
                        if (quoteListJson.quote_items.product.code == materialNumber) {
                            var quantity = quoteListJson.quote_items.quantity;
                            var quoteId = quoteListJson.ecom_qt_id_h;
                            //var price = findPriceByQuantity(quoteListJson.quote_items.product.volumePrices, quoteListJson.quote_items.quantity);
                            price = (quoteListJson.quote_items.requote_price * 1).toFixed(3);
                            if (userType == "DISTRIBUTOR") {
                                price = (quoteListJson.quote_items.adj_disti_cost * 1).toFixed(3);
                                quoteId = quoteListJson.modelNQuoteNumber;
                            }
                            var total = (price * quantity).toFixed(3);
                            if (quoteListJson.quote_items.quoteItemStatus == "ACTIVE" && quantity != "" && quantity != "undefined") {
                                activeFlag = 1;
                                useQuoteJson.quotesList.push({
                                    "totalCost": total,
                                    "quoteItemId": quoteListJson.quote_items.code,
                                    "status": quoteListJson.quote_items.quoteItemStatus,
                                    "quantity": quantity,
                                    "entryNumber": quoteListJson.quote_items.item_num,
                                    "quoteId": quoteId,
                                    "price": price,
                                    "orderCompilance": quoteListJson.quote_items.ordcompliance
                                });
                            }
                        } else {
                            continue;
                        }
                    } else {
                        for (var i = 0; i < quoteListJson.quote_items.length; i++) {
                            if (quoteListJson.quote_items[i].product.code == materialNumber) {
                                quantity = quoteListJson.quote_items[i].quantity;
                                var quoteId = quoteListJson.ecom_qt_id_h;
                                //var price = findPriceByQuantity(quoteListJson.quote_items[i].product.volumePrices, quoteListJson.quote_items[i].quantity);
                                price = (quoteListJson.quote_items[i].requote_price * 1).toFixed(3);
                                if (userType == "DISTRIBUTOR") {
                                    price = (quoteListJson.quote_items[i].adj_disti_cost * 1).toFixed(3);
                                    quoteId = quoteListJson.modelNQuoteNumber;
                                }
                                var total = price * quantity;
                                total = total.toFixed(3);
                                if (quoteListJson.quote_items[i].quoteItemStatus == "ACTIVE" && quantity != "" && quantity != "undefined") {
                                    activeFlag = 1;
                                    useQuoteJson.quotesList.push({
                                        "totalCost": total,
                                        "quoteItemId": quoteListJson.quote_items[i].code,
                                        "status": quoteListJson.quote_items.quoteItemStatus,
                                        "quantity": quantity,
                                        "entryNumber": quoteListJson.quote_items[i].item_num,
                                        "quoteId": quoteId,
                                        "price": price,
                                        "orderCompilance": quoteListJson.quote_items[i].ordcompliance
                                    });
                                }
                            } else {
                                continue;
                            }
                        }
                    }
                }
            };
            //alert(JSON.stringify(useQuoteJson));
            // alert(useQuoteJson);
            if (activeFlag == 1) {
                $("#quoteLineFlag" + index).val("true");
            } else {
                $("#quoteLineFlag" + index).val("false");
            }
            $("#modelBuyCartButton").trigger("click");
            $('#useQuoteModal').html(useQuoteTemplate(useQuoteJson));
            $('#useQuoteModal').modal('show');
        }
    });
    $("#useQuoteModal").modal("show");
}

 $(function() {
        // Stick the #nav to the top
        var nav = $('#nav');
		
		if(typeof nav.offset() == "undefined")
            return;
        
		var navHomeY = nav.offset().top-45;
        var isFixed = false;
        var $w = $(window);
        var navtop = 0;

        function load() {
            if (document.getElementById("Cookieconsent").style.display == "block") {
                navHomeY -= 27;
                navtop += 27;
            };
            // Set browser width
            $(document).ready(function() {
                defwid = $('#midarea').width();
                nav.css({
                    width: defwid,
                });
            });
			
			$('#Cookieconsent .closeX').on("click", function() {
				navtop = 0;
			});
        }

        // Resize browser width
        $(window).resize(function() {
            wid = $('#midarea').width();
            nav.css({
                width: wid,
            });
        });

        window.onload = load;

        $w.scroll(function() {
            // Scrolling/Floating Menu
            var scrollTop = $w.scrollTop();
            var shouldBeFixed = scrollTop > navHomeY;
            if (shouldBeFixed && !isFixed) {

                // Resize browser width
                $(window).resize(function() {
                    wid = $('#midarea').width();
                    nav.css({
                        width: wid,
                    });
                });

                nav.css({
                    position: 'fixed',
                    top: navtop,
					'padding-top': '5px',
					'background-color': '#fff'
                });

                isFixed = true;
            } else if (!shouldBeFixed && isFixed) {
                nav.css({
                    position: 'static',
                    width: nav.width(),
					'padding-top': '0px'
                });
                isFixed = false;
            }
        });
    });

$(window).scroll(function() {
    // Active Menu
    var position = $(this).scrollTop();
	var isAnyActive = false;
    $('.section').each(function() {
        var target = $(this).offset().top-85;
        var id = $(this).attr('id');
        if (position > target) {
			isAnyActive=true;
            $('#nav > a').removeClass('active');
            if (typeof id != 'undefined') {
                $('#nav > a[href="#' + id + '"]').addClass('active');
            }
        }
    });
	if(!isAnyActive) {
		$('#nav > a').removeClass('active');
	}

    $(window).resize(function() {
        if ($(window).width() < 767) {
            $('#nav').removeClass('btn-block');
            $('#nav').removeClass('btn-group');
        } else if ($(window).width() > 767) {
            $('#nav').addClass('btn-block');
            $('#nav').addClass('btn-group');
        }
    });
});


$(window).load(function() {
	$(function(){
    $.each(document.images, function(){
               var this_image = this;
               var src = $(this_image).attr('src') || '' ;
			  if(!src.length > 0){
                  var lsrc = $(this_image).attr('data-src') || '' ;
                   if(lsrc.length > 0){
                       this_image.src = lsrc;
                   }
               }
           });
  });
})



if (!String.prototype.includes) {
    String.prototype.includes = function() {
        'use strict';
        return String.prototype.indexOf.apply(this, arguments) !== -1;
    };
}
