function getCustomerInfo() {
 try{
   if(localStorage.getItem("getCustInfo") != 1 && (window.location.href.indexOf('storefront') > 0 || window.location.href.indexOf('products') > 0 || window.location.href.indexOf('design') > 0) && CQ_Analytics.PersonalInfoStoreMgr.getProperty("authorizableId") != "anonymous") {
        $.ajax({
            type: "get",
            url:'/bin/GetCustomerInformation',				
            async:false,
            success: function(json){
                localStorage.setItem("getCustInfo", "1");
                if(json.status != "false") {
					if(store != undefined && store.enabled && store.get("maxim_profile") != undefined) {
						store.get("maxim_profile").remove();
                        }                        
                                }
                                }
        });
	}}catch(e){
        console.log(e);						
    }
}

