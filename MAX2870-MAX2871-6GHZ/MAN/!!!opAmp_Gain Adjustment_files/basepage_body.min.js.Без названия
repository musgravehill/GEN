  $(document).ready(function() {
  $(".loading-screen").addClass("loaded");
  $(".header").bind("click", function() {});
  
    $(".hamburger").click(function() {
      window.scrollTo({
        top: 0,
        behavior: "smooth"
      }),
        $(this).toggleClass("active"),
        $(".page").toggleClass("lock-page"),
        $(".nav-menu.active").removeClass("active"),
        $(".nav-mobile").toggleClass("active");
    }),
      $(".nav-link").click(function() {
        if (
          $(this)
            .next()
            .is("ul.nav-menu")
        ) {
          $(".nav-menu").removeClass("active");
          $(this)
            .next("ul")
            .toggleClass("active");
        }
		
		if (
          $(this)
            .closest('.nav-item')
            .is("div.nav-item")
        ) {
          $(".nav-item").not($(this).closest('.nav-item')).removeClass("active");
          $(this).closest('.nav-item').toggleClass("active");
        }
      }),

      $(document).mouseup(function (e) {
        if ($(e.target).closest(".nav-menu").length === 0) {
          $(".nav-item").removeClass("active");
        }
      });

      $(".back-button").click(function() {
        $(this)
          .parent()
          .removeClass("active");
      }),
      $(window).resize(function() {
        1024 < $(document).width() &&
          $(".nav-mobile").has(".active") &&
          ($(".nav-mobile").removeClass("active"),
          $(".hamburger").removeClass("active"),
          $(".page").removeClass("lock-page"));
      });
  }),
  $(window).on("load", function() {
    
    if($(window).width() < 480)
    {
		$(".products-carousel").addClass("disabled");
        $(".application-carousel").addClass("disabled");
    }   
  });

//"object-fit:cover" alternative for banner in IE.
function msieversion() {
  var ua = window.navigator.userAgent;
  var msie = ua.indexOf("MSIE ");

  if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)) {
    $(".jumbotron-item").each(function() {
      var $container = $(this),
        imgUrl = $container.find("img").attr("data-src");
      if (imgUrl) {
        $container
          .css("backgroundImage", "url(" + imgUrl + ")")
          .addClass("compat-object-fit");
      }
    });
  }
  return false;
}

$(document).ready(function () {
  msieversion();

  var WeChatQRCode = $("#wechat_anchor").data("wechat-qr");
  if ("zh" == getUrlLanguage()) {
    $("#wechat_anchor").popover({
      html: true,
      placement: "top",
      trigger: "hover",
      content: function () {
        return '<img src="' + WeChatQRCode + '" />';
      }
    });
  }
});
