  $(".jumbotron-carousel").owlCarousel({
    loop: !0,
    margin: 10,
    items: 1,
    nav: !1,
    dots: !0,
    autoplay: !0,
    autoplaySpeed: 1e3,
    autoplayTimeout: carouselPlayspeed
  }),
    480 < $(window).width()
      ? startCarousels(80)
      : startCarousels(0);
}),
  $(window).resize(function() {
    480 < $(window).width()
      ? startCarousels(80)
      : startCarousels(0);
  }),
