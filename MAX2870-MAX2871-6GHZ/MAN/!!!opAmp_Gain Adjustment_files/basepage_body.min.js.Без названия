function startCarousels(lMargin) {

    $(".products-carousel").owlCarousel({
        loop: !1,
        responsiveClass: !0,
        nav: !0,
        navText: ["<div class='nav-icon'></div>", "<div class='nav-icon'></div>"],
        dots: !1,
        autoWidth: !0,
        margin: lMargin
    }), 
        $(".application-carousel").owlCarousel({
            loop: !1,
            responsiveClass: !0,
            nav: !0,
            navText: ["<div class='nav-icon'></div>", "<div class='nav-icon'></div>"],
            dots: !1,
            autoWidth: !0,
            margin: lMargin
        }),

		$(".products-carousel").removeClass("no-carousel"),
		$(".application-carousel").removeClass("no-carousel");

}

function stopCarousel(e) {


  var t = $(e);
  t.trigger("destroy.owl.carousel"), t.addClass("no-carousel");


}
var carouselPlayspeed = 5000;
carouselPlayspeed = $(".jumbotron-item").data("carousel-playspeed") * 1000;

function setColumns() {


    if ($(window).width() > 1199) {
      $('.navigation-submenus').each(function () {
       
	   
	    var keypadContainer =  $(this).parent().find(".keypad-tool-container");
        var keypadContainerLength = $(this).parent().find(".keypad-tool-container div").length;
		$(this).parent().css("display","block");
		if(keypadContainerLength > 1) {
			$(keypadContainer).css("width","25%");
			$(this).css("column-count",4);
			
		} else {
			$(keypadContainer).css("display","none");
			$(this).css("column-count",5);
			$(this).height($(this).height() - 1);
		}
		$(this).parent().css("display","flex");
      })
    } else if($(window).width() > 1023) {
		$('.navigation-submenus').each(function () {
			 $(this).parent().css("display","block");
			 $(this).css("column-count",3);
			 $(this).css("height","auto");
			 $(this).parent().css("display","flex");
		});			
		
	} else if($(window).width() > 767) {
		
		$('.navigation-submenus').each(function () {
			 $(this).css("column-count",2);
			 $(this).css("height","auto");
			 $(this).parent().css("display","block");
		});
	} else {
		
		$('.navigation-submenus').each(function () {
			 $(this).css("column-count",1);
			 $(this).css("height","auto");
			 $(this).parent().css("display","block");
		});
	}

}

