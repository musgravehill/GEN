Array.prototype.contains=function(a){for(var b=0;b<this.length;b++)if(this[b]===a)return!0;return!1};Array.prototype.unique=function(){for(var a=[],b=0;b<this.length;b++)a.includes(this[b])||a.push(this[b]);return a};String.prototype.replaceAll=function(a,b){return this.replace(new RegExp(a,"g"),b)};